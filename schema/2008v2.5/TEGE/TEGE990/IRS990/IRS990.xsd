			<xsd:element name="InvestmentIncome" type="Form990PartVIIIGroup3Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment income</Description>
						<LineNumber>Part VIII Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

