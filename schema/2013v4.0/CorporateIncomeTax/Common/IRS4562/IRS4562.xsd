						<LineNumber>40</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Meet Requirements Concerning Qualified Automobile Demonstration Use -->
			<xsd:element name="MeetRqrForAutoDemoUseInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Do you meet the requirements concerning qualified automobile demonstration use?</Description>
						<LineNumber>41</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Part VI - Amortization ===== -->

			<!-- Amortization Information -->
			<xsd:element name="AmortizationInfo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amortization information</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="AmortizationInfoTable" type="AmortizationInfoTableType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amortization information table</Description>
