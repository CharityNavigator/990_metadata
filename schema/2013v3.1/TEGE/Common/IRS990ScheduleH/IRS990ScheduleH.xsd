		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for all Count type elements (Number of something counts/tallys) -->
	<xsd:simpleType name="Count2Type">
		<xsd:restriction base="IntegerNNType">
			<xsd:totalDigits value="6"/>
		</xsd:restriction>
	</xsd:simpleType>

</xsd:schema>
