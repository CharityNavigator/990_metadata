<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.irs.gov/efile" xmlns="http://www.irs.gov/efile"
xmlns:xsd="http://www.w3.org/2001/XMLSchema"
elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">

	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - Form2220</Description>
			<TaxYear>2007</TaxYear>
			<MaturityLevel>Draft Release</MaturityLevel>
			<ReleaseDate>August 17, 2007</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd" />

	<!-- =============================================================== -->
	<!-- =======================  IRS Form 2220  ======================= -->
	<!-- =============================================================== -->

	<xsd:element name="IRS2220">
		<xsd:annotation>
			<xsd:documentation>IRS Form 2220</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS2220Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="StringType" fixed="IRS2220">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS2220Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 2220</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- ===== Required Annual Payment ===== -->

			<!-- Total Tax -->
			<xsd:element name="TotalTax" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax</Description>
						<LineNumber>1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="section338GainIndicator">
								<xsd:simpleType>
									<xsd:restriction base="StringType">
										<xsd:enumeration value="SEC. 338 GAIN" />
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="section338GainAmount" type="USAmountType"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Personal Holding Company Tax -->
			<xsd:element name="PersonalHoldingCompanyTax" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Personal holding company tax included in total tax</Description>
						<LineNumber>2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Lookback Interest Included -->
			<xsd:element name="LookbackInterestIncluded" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Look-back interest included in total tax under section 460(b)(2) for completed long-term contracts or section 167(g) for depreciation under the income forecast method</Description>
						<LineNumber>2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Credit for Federal Tax Paid on Fuels -->
			<xsd:element name="CreditForFederalTaxPaidOnFuels" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Credit for federal tax paid on fuels</Description>
						<LineNumber>2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total -->
			<xsd:element name="SumOfIncludedTaxesAndCredits" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
							<Description>Total.  Add lines 2a through 2c</Description>
						<LineNumber>2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax Less Sum of Included Taxes and Credits -->
			<xsd:element name="TotalTaxLessSumOfIncluded" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subtract line 2d from line 1</Description>
						<LineNumber>3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Shown on Prior Year Tax Return -->
			<xsd:element name="TaxShownOnPriorYearTaxReturn" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax shown on the corporation's prior year income tax return</Description>
						<LineNumber>4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Smaller Tax -->
			<xsd:element name="SmallerTax" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the smaller of line 3 or line 4</Description>
						<LineNumber>5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Corporation is Using the Adjusted Seasonal Installment Method -->
			<xsd:element name="UsingAdjustedSeasonal" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The corporation is using the adjusted seasonal installment method</Description>
						<LineNumber>6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Corporation is Using the Annualized Income Installment Method -->
			<xsd:element name="UsingAnnualizedIncome" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The corporation is using the annualized income installment method</Description>
						<LineNumber>7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Large Corporation figuring its 1st Required Installment based on the Prior Year's Tax -->
			<xsd:element name="LargeCorporation1stInstallment" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The corporation is a "large corporation" figuring its first required installment based on the prior year's tax</Description>
						<LineNumber>8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Installment Due Date #1 -->
			<xsd:element name="InstallmentDueDate1" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter in column the 15th day of the 4th, 6th, 9th, and 12th months of the corp tax year</Description>
						<LineNumber>9a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment for Installment Due Date #1 -->
			<xsd:element name="RequiredInstallment1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Required installment for installment due date #1</Description>
						<LineNumber>10a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Estimated Tax Paid or Credited for Installment Due Date #1 -->
			<xsd:element name="EstimatedTaxPaidOrCredited1" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estimated tax paid or credited for installment due date #1</Description>
						<LineNumber>11a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 15a, use value of 11a (Estimated Tax Paid or Credited for Installment Due Date #1) -->

			<!-- Underpayment for Installment Due Date #1 -->
			<xsd:element name="Underpayment1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment.  If line 15 is less than or equal to line 10, subtract line 15 from line 10</Description>
						<LineNumber>17a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Overpayment for Installment Due Date #1 -->
			<xsd:element name="Overpayment1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Overpayment for installment due date #1</Description>
						<LineNumber>18a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Installment Due Date #2 -->
			<xsd:element name="InstallmentDueDate2" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter in column the 15th day of the 4th, 6th, 9th, and 12th months of the corp tax year</Description>
						<LineNumber>9b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment for Installment Due Date #2 -->
			<xsd:element name="RequiredInstallment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Required installment for installment due date #2</Description>
						<LineNumber>10b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Estimated Tax Paid or Credited for Installment Due Date #2 -->
			<xsd:element name="EstimatedTaxPaidOrCredited2" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estimated tax paid or credited for installment due date #2</Description>
						<LineNumber>11b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 12b, use value of 18a (Overpayment for Installment Due Date #1) -->

			<!-- Sum of Estimated Tax Paid or Credited for Installment Due Date #2 and Overpayment for Installment Due Date #1 -->
			<xsd:element name="EstimatedPaid2AndOverpayment1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 11 and 12</Description>
						<LineNumber>13b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 14b, use value of 17a.  This value will be considered the Total Underpayment for Installment Due Date #1 -->

			<!-- Sum of Estimated Tax Paid or Credited for Installment Due Date #2 and Overpayment for Installment Due Date #1 minus Total Underpayment for Installment Due Date #1 -->
			<xsd:element name="EstPd2AndOvpmt1LessTotUndpymt1" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subtract line 14 from line 13</Description>
						<LineNumber>15b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Underpayment Carryover from Installment Due Date #1 for Installment Due Date #2 -->
			<xsd:element name="UnderpaymentCarryover2" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If the amount on line 15 is zero, subtract line 13 from line 14</Description>
						<LineNumber>16b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Underpayment for Installment Due Date #2 -->
			<xsd:element name="Underpayment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment.  If line 15 is less than or equal to line 10, subtract line 15 from line 10</Description>
						<LineNumber>17b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Overpayment for Installment Due Date #2 -->
			<xsd:element name="Overpayment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Overpayment for installment due date #2</Description>
						<LineNumber>18b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Installment Due Date #3 -->
			<xsd:element name="InstallmentDueDate3" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter in column the 15th day of the 4th, 6th, 9th, and 12th months of the corp tax year</Description>
						<LineNumber>9c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment for Installment Due Date #3 -->
			<xsd:element name="RequiredInstallment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Required installment for installment due date #3</Description>
						<LineNumber>10c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Estimated Tax Paid or Credited for Installment Due Date #3 -->
			<xsd:element name="EstimatedTaxPaidOrCredited3" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estimated tax paid or credited for installment due date #3</Description>
						<LineNumber>11c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 12c, use value of 18b (Overpayment for Installment Due Date #2) -->

			<!-- Sum of Estimated Tax Paid or Credited for Installment Due Date #3 and Overpayment for Installment Due Date #2 -->
			<xsd:element name="EstimatedPaid3AndOverpayment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 11 and 12</Description>
						<LineNumber>13c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Underpayment from Installment Due Date #2 -->
			<xsd:element name="TotalUnderpayment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total underpayment from preceding column (for installment due date #2, line 16b plus line 17b)</Description>
						<LineNumber>14c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Sum of Estimated Tax Paid or Credited for Installment Due Date #3 and Overpayment for Installment Due Date #2 less Total Underpayment for Installment Due Date #2 -->
			<xsd:element name="EstPd3AndOvpmt2LessTotUndpymt2" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subtract line 14 from line 13</Description>
						<LineNumber>15c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Underpayment Carryover from Installment Due Date #2 for Installment Due Date #3 -->
			<xsd:element name="UnderpaymentCarryover3" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If the amount on line 15 is zero, subtract line 13 from line 14</Description>
						<LineNumber>16c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Underpayment for Installment Due Date #3 -->
			<xsd:element name="Underpayment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment.  If line 15 is less than or equal to line 10, subtract line 15 from line 10</Description>
						<LineNumber>17c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Overpayment for Installment Due Date #3 -->
			<xsd:element name="Overpayment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Overpayment for installment due date #3</Description>
						<LineNumber>18c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Installment Due Date #4 -->
			<xsd:element name="InstallmentDueDate4" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter in column the 15th day of the 4th, 6th, 9th, and 12th months of the corp tax year</Description>
						<LineNumber>9d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment for Installment Due Date #4 -->
			<xsd:element name="RequiredInstallment4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Required installment for installment due date #4</Description>
						<LineNumber>10d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Estimated Tax Paid or Credited for Installment Due Date #4 -->
			<xsd:element name="EstimatedTaxPaidOrCredited4" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estimated tax paid or credited for installment due date #4</Description>
						<LineNumber>11d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 12d, use value of 18c (Overpayment for Installment Due Date #3) -->

			<!-- Sum of Estimated Tax Paid or Credited for Installment Due Date #4 and Overpayment for Installment Due Date #3 -->
			<xsd:element name="EstimatedPaid4AndOverpayment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 11 and 12</Description>
						<LineNumber>13d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Underpayment from Installment Due Date #3 -->
			<xsd:element name="TotalUnderpayment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total underpayment from preceding column (for installment due date #3, line 16c plus line 17c)</Description>
						<LineNumber>14d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Sum of Estimated Tax Paid or Credited for Installment Due Date #4 and Overpayment for Installment Due Date #3 less Total Underpayment for Installment Due Date #3 -->
			<xsd:element name="EstPd4AndOvpmt3LessTotUndpymt3" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subtract line 14 from line 13</Description>
						<LineNumber>15d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Underpayment for Installment Due Date #4 -->
			<xsd:element name="Underpayment4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment.  If line 15 is less than or equal to line 10, subtract line 15 from line 10</Description>
						<LineNumber>17d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Figuring the Penalty ===== -->

			<!-- Payment Penalty Computation table -->
			<xsd:element name="PaymentPenaltyComputation" type="PaymentPenaltyComputationType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Payment penalty computation</Description>
						<LineNumber>19a - 33a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty Amount -->
			<xsd:element name="TotalPenalty" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Penalty Amount</Description>
						<LineNumber>34</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- IRS2220 Schedule A - Annualized Income Installment Method and/or the Adjusted Seasonal Installment Method Under Section 6655(e) -->
			<xsd:element name="IRS2220ScheduleA" type="IRS2220ScheduleAType" minOccurs="0" />

		</xsd:sequence>

	</xsd:complexType>

	<!-- =============================================================== -->
	<!-- ===================  IRS Form 2220 Schedule A ================= -->
	<!-- =============================================================== -->

	<xsd:complexType name="IRS2220ScheduleAType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 2220 Schedule A</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- ===== Part I - Adjusted Seasonal Installment Method ===== -->

			<!-- Taxable Income for Tax Year #1 for 1st 3 months -->
			<xsd:element name="First3MonthsTaxableIncomeTY1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #1 for 1st 3 months</Description>
						<LineNumber>Schedule A Line 1a(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #2 for 1st 3 months -->
			<xsd:element name="First3MonthsTaxableIncomeTY2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #2 for 1st 3 months</Description>
						<LineNumber>Schedule A Line 1b(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #3 for 1st 3 months -->
			<xsd:element name="First3MonthsTaxableIncomeTY3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #3 for 1st 3 months</Description>
						<LineNumber>Schedule A Line 1c(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Current Tax Year for 1st 3 months -->
			<xsd:element name="First3MonthsTaxableIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for current tax year for 1st 3 months</Description>
						<LineNumber>Schedule A Line 2(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #1 for 1st 5 months -->
			<xsd:element name="First5MonthsTaxableIncomeTY1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #1 for 1st 5 months</Description>
						<LineNumber>Schedule A Line 1a(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #2 for 1st 5 months -->
			<xsd:element name="First5MonthsTaxableIncomeTY2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #2 for 1st 5 months</Description>
						<LineNumber>Schedule A Line 1b(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #3 for 1st 5 months -->
			<xsd:element name="First5MonthsTaxableIncomeTY3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #3 for 1st 5 months</Description>
						<LineNumber>Schedule A Line 1c(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Current Tax Year for 1st 5 months -->
			<xsd:element name="First5MonthsTaxableIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for current tax year for 1st 5 months</Description>
						<LineNumber>Schedule A Line 2(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #1 for 1st 8 months -->
			<xsd:element name="First8MonthsTaxableIncomeTY1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #1 for 1st 8 months</Description>
						<LineNumber>Schedule A Line 1a(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #2 for 1st 8 months -->
			<xsd:element name="First8MonthsTaxableIncomeTY2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #2 for 1st 8 months</Description>
						<LineNumber>Schedule A Line 1b(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #3 for 1st 8 months -->
			<xsd:element name="First8MonthsTaxableIncomeTY3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #3 for 1st 8 months</Description>
						<LineNumber>Schedule A Line 1c(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Current Tax Year for 1st 8 months -->
			<xsd:element name="First8MonthsTaxableIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
