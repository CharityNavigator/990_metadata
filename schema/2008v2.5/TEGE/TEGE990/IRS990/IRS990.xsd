						<xsd:element name="AverageHoursPerWeek" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Average hours per week</Description>
									<LineNumber>Part VII Section A Line 1a B</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="IntegerNNType">
									<xsd:minInclusive value="001"/>
									<xsd:maxInclusive value="168"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>

						<!-- Individual Trustee or Director -->
						<xsd:element name="IndividualTrusteeOrDirector" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Individual trustee or director</Description>
									<LineNumber>Part VII Section A Line 1a C</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Institutional Trustee -->
						<xsd:element name="InstitutionalTrustee" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Institutional Trustee</Description>
									<LineNumber>Part VII Section A Line 1a C</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Officer -->
						<xsd:element name="Officer" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Officer</Description>
									<LineNumber>Part VII Section A Line 1a C</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Key Employee -->
						<xsd:element name="KeyEmployee" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Key Employee</Description>
									<LineNumber>Part VII Section A Line 1a C</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Highest Compensated Employee -->
						<xsd:element name="HighestCompensatedEmployee" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Highest compensated employee</Description>
									<LineNumber>Part VII Section A Line 1a C</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Former -->
						<xsd:element name="Former" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Former</Description>
									<LineNumber>Part VII Section A Line 1a C</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Reportable Compensation From Organization -->
						<xsd:element name="ReportableCompFromOrganization" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Reportable compensation from organization</Description>
									<LineNumber>Part VII Section A Line 1a D</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

