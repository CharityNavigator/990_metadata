		</xsd:sequence>

	</xsd:complexType>

	<!-- ===== Type for Repeating Element "AmortizationInfoTable" in Part VI ===== -->

	<xsd:complexType name="AmortizationInfoTableType">

		<xsd:sequence>

			<!-- Description of Costs -->
			<xsd:element name="CostDesc" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description of costs</Description>
						<LineNumber>42a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Date Amortization Begins -->
			<xsd:element name="AmortizationBeginDt" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Date amortization begins</Description>
						<LineNumber>42b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amortizable Amount -->
			<xsd:element name="AmortizableAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amortization amount</Description>
						<LineNumber>42c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Code Section -->
			<xsd:element name="CodeSectionTxt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Code section</Description>
						<LineNumber>42d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
