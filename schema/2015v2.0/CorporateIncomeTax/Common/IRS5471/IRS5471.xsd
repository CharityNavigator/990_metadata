				</xsd:complexType>
			</xsd:element>

			<!-- End Accounting Period Other Liabilities -->
			<xsd:element name="EndAcctPrdOthLiabilitiesAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period other liabilities</Description>
						<LineNumber>Schedule F Line 17(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="ItemizedOtherLiabilitiesSchedule"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Beginning Accounting Period Preferred Stock -->
			<xsd:element name="BegngAcctPrdPreferredStockAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period preferred stock</Description>
						<LineNumber>Schedule F Line 18a(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Preferred Stock -->
			<xsd:element name="EndAcctPrdPreferredStockAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period preferred stock</Description>
						<LineNumber>Schedule F Line 18a(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Beginning Accounting Period Common Stock -->
			<xsd:element name="BegngAcctPrdCommonStockAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period common stock</Description>
						<LineNumber>Schedule F Line 18b(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Common Stock -->
			<xsd:element name="EndAcctPrdCommonStockAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period common stock</Description>
						<LineNumber>Schedule F Line 18b(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Beginning Accounting Period Paid-in or Capital Surplus -->
			<xsd:element name="BegngAcctPrdPaidInOrSurplusAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period paid-in or capital surplus</Description>
						<LineNumber>Schedule F Line 19(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="PaidInOrCapSurplusRecnclStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Beginning Accounting Period Paid-in or Capital Surplus -->
			<xsd:element name="EndAcctPrdPaidInOrSurplusAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period paid-in or capital surplus</Description>
						<LineNumber>Schedule F Line 19(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="PaidInOrCapSurplusRecnclStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Beginning Accounting Period Retained Earnings -->
			<xsd:element name="BegngAcctPrdRtnEarningsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period retained earnings</Description>
						<LineNumber>Schedule F Line 20(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Retained Earnings -->
			<xsd:element name="EndAcctPrdRtnEarningsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period retained earnings</Description>
						<LineNumber>Schedule F Line 20(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Beginning Accounting Period Retained Earnings Less Cost of Treasury Stock -->
			<xsd:element name="BegngAcctPrdNetCostOfTrStkAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period retained earnings less cost of treasury stock</Description>
						<LineNumber>Schedule F Line 21(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Retained Earnings Less Cost of Treasury Stock -->
			<xsd:element name="EndAcctPrdNetCostOfTrStkAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period retained earnings less cost of treasury stock</Description>
						<LineNumber>Schedule F Line 21(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Beginning Accounting Period Total Liabilities and Stockholders' Equity -->
			<xsd:element name="BegngAcctPrdTotLiabShrEqtyAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period total liabilities and stockholders' equity</Description>
						<LineNumber>Schedule F Line 22(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Total Liabilities and Stockholders' Equity -->
			<xsd:element name="EndAcctPrdTotLiabShrEqtyAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period total liabilities and stockholders' equity</Description>
						<LineNumber>Schedule F Line 22(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule G ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleGType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1120 Schedule G</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>
			<!-- Foreign Corporation Owns 10% or More in a Foreign Partnership -->
			<xsd:element name="Owns10PctOrMoreFrgnPrtshpInd">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation own 10% or more in a foreign partnership</Description>
						<LineNumber>Schedule G Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="OwnsForeignPartnershipStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Foreign Corporation Owns Interest in Any Trust -->
			<xsd:element name="FrgnCorpOwnsInterestInTrustInd" type="BooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation owns interest in any trust</Description>
						<LineNumber>Schedule G Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Corporation Owns Any Foreign Entities -->
			<xsd:element name="FrgnCorpOwnsForeignEntityInd">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation owns any foreign entities</Description>
						<LineNumber>Schedule G Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS8858 OwnsForeignEntitiesStatement"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- The Foreign Corporation Was a Participant in a Cost Sharing Arrangement -->
			<xsd:element name="FrgnCorpPartcpCostShrInd" type="BooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The foreign corporation was a participant in a cost sharing arrangement</Description>
						<LineNumber>Schedule G Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- The Foreign Corporation Became a Participant in a Cost Sharing Arrangement -->
			<xsd:element name="FrgnCorpBecamePartcpCostShrInd" type="BooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The foreign corporation became a participant in a cost sharing arrangement</Description>
						<LineNumber>Schedule G Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- During the Tax Year, did the Foreign Corporation Participate in any Reportable Transaction as Defined in Regulations Section 1.6011-4? -->
			<xsd:element name="ReportableTransactionPrtcptInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>During the tax year, did the foreign corporation participate in any reportable transaction as defined in Regulations section 1.6011-4?</Description>
						<LineNumber>Schedule G Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS8886"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- During the Tax Year, did the Foreign Corporation pay or Accrue any Foreign Tax That was Disqualified for Credit Under Section 901(m)? -->
			<xsd:element name="FrgnTaxDisqualifiedSec901mInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>During the tax year, did the foreign corporation pay or accrue any foreign tax that was disqualified for credit under section 901(m)?</Description>
						<LineNumber>Schedule G Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- During the Tax Year, did the Foreign Corporation pay or Accrue Foreign Taxes to Which Section 909 Applies, or Treat Foreign Taxes That Were Previously Suspended Under Section 909 as no Longer Suspended? -->
			<xsd:element name="ForeignTaxSection909Ind" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>During the tax year, did the foreign corporation pay or accrue foreign taxes to which section 909 applies, or treat foreign taxes that were previously suspended under section 909 as no longer suspended?</Description>
						<LineNumber>Schedule G Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule H ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleHType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1120 Schedule H</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>
			<!-- Current Year Income Per Foreign Books of Account -->
			<xsd:element name="ForeignCYNetIncomePerBooksAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year income per foreign books of account </Description>
						<LineNumber>Schedule H Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Capital Gains Or Losses Net Additions -->
			<xsd:element name="CapitalGainOrLossNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital gains or losses net additions</Description>
						<LineNumber>Schedule H Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Capital Gains Or Losses Net Subtractions -->
			<xsd:element name="CapitalGainOrLossNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital gains or losses net subtractions</Description>
						<LineNumber>Schedule H Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depreciation and Amortization Net Additions -->
			<xsd:element name="DeprecAndAmortzNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depreciation and amortization net additions</Description>
						<LineNumber>Schedule H Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depreciation and Amortization Net Subtractions -->
			<xsd:element name="DeprecAndAmortzNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depreciation and amortization net subtractions</Description>
						<LineNumber>Schedule H Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depletion Net Additions -->
			<xsd:element name="DepletionNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depletion net additions</Description>
						<LineNumber>Schedule H Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depletion Net Subtraction -->
			<xsd:element name="DepletionNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depletion net subtraction</Description>
						<LineNumber>Schedule H Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investment or Incentive Allowance Net Additions -->
			<xsd:element name="InvstIncntvAllwncNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment or incentive allowance net additions</Description>
						<LineNumber>Schedule H Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investment or Incentive Allowance Net Subtracions -->
			<xsd:element name="InvstIncntvAllwncNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment or incentive allowance net subtraction</Description>
						<LineNumber>Schedule H Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charges to Statutory Reserves Net Additions -->
			<xsd:element name="ChrgsStryReservesNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charges to statutory reserves net additions</Description>
						<LineNumber>Schedule H Line 2e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charges to Statutory Reserves Net Subtractions -->
			<xsd:element name="ChrgsStryReservesNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charges to statutory reserves net subtractions</Description>
						<LineNumber>Schedule H Line 2e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Inventory Adjustments Net Additions -->
			<xsd:element name="InventoryAdjNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Inventory adjustments net additions</Description>
						<LineNumber>Schedule H Line 2f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Inventory Adjustments Net Subtractions -->
			<xsd:element name="InventoryAdjNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Inventory adjustments net subtractions</Description>
						<LineNumber>Schedule H Line 2f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxes Net Additions -->
			<xsd:element name="TaxesNetAddnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxes net additions</Description>
						<LineNumber>Schedule H Line 2g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxes Net Subtractions -->
			<xsd:element name="TaxesNetSbtrctnAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxes net subtractions</Description>
						<LineNumber>Schedule H Line 2g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Adjustments Net Additions -->
			<xsd:element name="OtherAdjustmentsNetAddnAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other adjustments net additions</Description>
						<LineNumber>Schedule H Line 2h</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="ForeignAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="EarningsAndProfitsOtherAdjStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Other Adjustments Net Subtractions -->
			<xsd:element name="OtherAdjustmentsNetSbtrctnAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other adjustments net subtractions</Description>
						<LineNumber>Schedule H Line 2h</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="ForeignAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="EarningsAndProfitsOtherAdjStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Net Additions -->
			<xsd:element name="TotalNetAdditonsAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net additions</Description>
						<LineNumber>Schedule H Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Net Subtractions -->
			<xsd:element name="TotalNetSubtractionsAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net subtractions</Description>
						<LineNumber>Schedule H Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Earnings and Profits -->
			<xsd:element name="CurrentEarningsAndProfitsAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>current earnings and profits</Description>
						<LineNumber>Schedule H Line 5a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- DASTM Gain or Loss -->
			<xsd:element name="DASTMGainOrLossAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>DASTM gain or loss</Description>
						<LineNumber>Schedule H Line 5b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Earnings and Profits plus DASTM Gain -->
			<xsd:element name="EarningAndPrftPlusDASTMGainAmt" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>current earnings and profits plus DASTM gain</Description>
						<LineNumber>Schedule H Line 5c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Earnings and Profits in US Dollars -->
			<xsd:element name="CurrEarnAndPrftInUSDollarsAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current earnings and profits in US dollars</Description>
						<LineNumber>Schedule H Line 5d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="exchangeRt" type="LargeRatioType"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			
		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule I ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleIType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1120 Schedule I</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>	
		
			<!-- Shareholder Information-->
			<xsd:element name="ShareholderInformation" type="ShareholderInformationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Shareholder information</Description>
						<LineNumber>Schedule I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
				
			<!-- Subpart F Income -->
			<xsd:element name="SubpartFIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subpart F income</Description>
						<LineNumber>Schedule I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Earnings Invested in US Property -->
			<xsd:element name="EarningsInvestedInUSPropAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Earnings invested in US property</Description>
						<LineNumber>Schedule I Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Previously Excluded Subpart F Income-->
			<xsd:element name="PrevExcludedSubpartFIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Previously excluded subpart F income</Description>
						<LineNumber>Schedule I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Previously Excluded Export Trade Income Withrawn from Investment in Export Trade Asset -->
			<xsd:element name="WithdrawnExportTradeIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Previously excluded export trade income withrawn from investment in export trade asset</Description>
						<LineNumber>Schedule I Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Factoring Income -->
			<xsd:element name="FactoringIncomeAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Factoring income</Description>
						<LineNumber>Schedule I Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of lines 1 throug 5 -->
			<xsd:element name="TotalIncmControlledFrgnCorpAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of lines 1 through 5</Description>
						<LineNumber>Schedule I Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Dividends Received -->
			<xsd:element name="DividendsReceivedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Dividends received</Description>
						<LineNumber>Schedule I Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Exchange Gain on a Distribution of Previously Taxed Income -->
			<xsd:element name="ExchangeGainOrLossOnDistriAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exchange gain on a distribution of previously taxed income</Description>
						<LineNumber>Schedule I Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Was Any Income of the Foreign Corporation Blocked? -->
			<xsd:element name="IncomeBlockedInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Was any income of the foreign corporation blocked?</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="ExplanationOfBlockedIncomeStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Did Any Income Become Unblocked This Tax Year? -->
			<xsd:element name="IncomeUnblockedInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did any income become unblocked this tax year?</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="ExplanationOfBlockedIncomeStmt"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- ===== Type for Element "ShareholderInformation" ===== -->
	
	<xsd:complexType name="ShareholderInformationType">
		<xsd:annotation>
			<xsd:documentation>Content model for Shareholder Information for Form 1120 Schedule I</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Choice for Name of US Shareholder: choice between Person Name or Business Name -->
			<xsd:choice minOccurs="0">

				<!-- Shareholder Person Name -->
				<xsd:element name="ShareholderPersonNm" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Shareholder person name</Description>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Shareholder Business Name -->
				<xsd:element name="ShareholderBusinessName" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Shareholder business name</Description>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

			</xsd:choice>	

			<!-- Choice for Identifying Number: choice between EIN or SSN or Missing EIN Reason -->
			<xsd:choice minOccurs="0">

				<!-- Shareholder EIN -->
				<xsd:element name="ShareholderEIN" type="EINType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Shareholder EIN</Description>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Shareholder SSN -->
				<xsd:element name="ShareholderSSN" type="SSNType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Shareholder SSN</Description>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Shareholder Missing EIN Reason  -->
				<xsd:element name="ShareholderMissingEINReasonCd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Shareholder missing EIN reason</Description>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="StringType">
							<xsd:enumeration value="APPLD FOR"/>
							<xsd:enumeration value="FOREIGNUS"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>

			</xsd:choice>

		</xsd:sequence>
	</xsd:complexType>

	<!-- ========================================================================== -->
	<!-- ======================  Recurring Local Types ============================ -->
	<!-- ========================================================================== -->

	<!-- Recurring Stock Description Type -->
	<xsd:simpleType name="StockDescriptionType">
		<xsd:annotation>
			<xsd:documentation>Recurring Stock Description Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="TextType">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Foreign Corporation Stocks Repeating Group Type -->
	<xsd:complexType name="ForeignCorporationStocksType">
		<xsd:sequence>

			<!-- Description of Stock Held by Shareholder -->
			<xsd:element name="StockClassDesc" type="StockDescriptionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description of stock held by shareholder</Description>
						<LineNumber>Schedule B Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Shares Issued Beginning Annual Accounting Period -->
			<xsd:element name="AnnualAcctPeriodBeginShareCnt" type="NumberOfSharesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of shares issued beginning annual accounting period</Description>
						<LineNumber>Schedule B Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Shares Issued End Annual Accounting Period -->
			<xsd:element name="AnnualAcctPeriodEndShareCnt" type="NumberOfSharesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of shares issued end annual accounting period</Description>
						<LineNumber>Schedule B Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>
