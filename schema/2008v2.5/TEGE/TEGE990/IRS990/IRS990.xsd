				<xsd:annotation>
					<xsd:documentation>
						<Description>Section B</Description>
						<LineNumber>Part VII Section B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Contractors GT $100K -->
			<xsd:element name="NumberOfContractorsGT100K" type="CountType" minOccurs="0">
