									</xsd:element>

									<!-- Facility Type -->
									<xsd:element name="FacilityTxt" type="LineExplanationType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Facility type</Description>
												<LineNumber>Part V Section D</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Part VI Supplemental Information -->
			<!-- Repeating Group -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Part VI contents</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
