				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide line 2 by line 8</Description>
						<LineNumber>Schedule A Line 9(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Figured for Taxable Income for Current Tax Year for 1st 9 months divided by Average of Taxable Income Ratios for 1st 9 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="First9MonthsTaxFigured1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Figure the tax on the amount on line 9 using the instructions for Form 1120.</Description>
						<LineNumber>Schedule A Line 10(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #1 for 1st 9 months divided by Taxable Income for Tax Year #1 for entire year -->
			<xsd:element name="First9MonthsIncomeRatio2TY1" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide the amount in columns (a) through (c) on line 3a by the amount in column (d) on line 3a</Description>
						<LineNumber>Schedule A Line 11a(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #2 for 1st 9 months divided by Taxable Income for Tax Year #2 for entire year -->
			<xsd:element name="First9MonthsIncomeRatio2TY2" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide the amount in columns (a) through (c) on line 3b by the amount in column (d) on line 3b</Description>
						<LineNumber>Schedule A Line 11b(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #3 for 1st 9 months divided by Taxable Income for Tax Year #3 for entire year -->
			<xsd:element name="First9MonthsIncomeRatio2TY3" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide the amount in columns (a) through (c) on line 3c by the amount in column (d) on line 3c</Description>
						<LineNumber>Schedule A Line 11c(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of Taxable Income Ratios for 1st 9 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="First9MonthsIncomeRatioTotal2" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 11a through 11c</Description>
						<LineNumber>Schedule A Line 12(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Average of Taxable Income Ratios for 1st 9 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="First9MonthsIncomeRatioAvg2" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide line 12 by 3</Description>
						<LineNumber>Schedule A Line 13(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 1st 9 months Current Tax Year, Average of Taxable Income Ratios for 1st 9 months of Tax Year #1 through Tax Year #3 multiplied by the Tax Figured for Taxable Income for Current Tax Year for 1st 8 months divided by Average of Taxable Income Ratios for 1st 8 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="First9MonthsTaxFigured2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Multiply the amount in columns (a) through (c) of line 10 by columns (a) through (c) of line 13</Description>
						<LineNumber>Schedule A Line 14(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Alternative Minimum Tax for 1st 9 months of Current Tax Year -->
			<xsd:element name="First9MonthsAlternativeMinTax" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter alternative minimum tax for each payment period</Description>
						<LineNumber>Schedule A Line 15(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Taxes for 1st 9 months of Current Tax Year -->
			<xsd:element name="First9MonthsOtherTaxes" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter other taxes for each payment period</Description>
						<LineNumber>Schedule A Line 16(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total tax for 1st 9 months of Current Tax Year -->
			<xsd:element name="First9MonthsTotalTax" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax.  Add lines 14 through 16</Description>
						<LineNumber>Schedule A Line 17(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Credits for 1st 9 months of Current Tax Year -->
			<xsd:element name="First9MonthsTaxCredits" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>For each period, enter the same type of credits as allowed on Form 2220, lines 4 and 5c</Description>
						<LineNumber>Schedule A Line 18(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax After Credits for 1st 9 months of Current Tax Year -->
			<xsd:element name="First9MonthsTaxAfterCredits" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax after credits</Description>
						<LineNumber>Schedule A Line 19(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #1 for Entire Year -->
			<xsd:element name="EntireYearTaxableIncomeTY1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #1 for entire year</Description>
						<LineNumber>Schedule A Line 3a(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #2 for Entire Year -->
			<xsd:element name="EntireYearTaxableIncomeTY2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #2 for entire year</Description>
						<LineNumber>Schedule A Line 3b(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #3 for Entire Year -->
			<xsd:element name="EntireYearTaxableIncomeTY3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for tax year #3 for entire year</Description>
						<LineNumber>Schedule A Line 3c(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #1 for 1st 11 months divided by Taxable Income for Tax Year #1 for entire year -->
			<xsd:element name="EntireYearIncomeRatioTY1" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide the amount in each column on line 1a by the amount in column (d) on line 3a</Description>
						<LineNumber>Schedule A Line 4(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #2 for 1st 11 months divided by Taxable Income for Tax Year #2 for entire year -->
			<xsd:element name="EntireYearIncomeRatioTY2" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide the amount in each column on line 1b by the amount in column (d) on line 3b</Description>
						<LineNumber>Schedule A Line 5(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Tax Year #3 for 1st 11 months divided by Taxable Income for Tax Year #3 for entire year -->
			<xsd:element name="EntireYearIncomeRatioTY3" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide the amount in each column on line 1c by the amount in column (d) on line 3c</Description>
						<LineNumber>Schedule A Line 6(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of Taxable Income Ratios for 1st 11 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="EntireYearIncomeRatioTotal" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 4 through 6</Description>
						<LineNumber>Schedule A Line 7(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Average of Taxable Income Ratios for 1st 11 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="EntireYearIncomeRatioAvg" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide line 7 by 3</Description>
						<LineNumber>Schedule A Line 8(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Income for Current Tax Year for 1st 11 months divided by Average of Taxable Income Ratios for 1st 11 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="EntireYearIncomeFactored" type="LargeRatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Divide line 2 by line 8</Description>
						<LineNumber>Schedule A Line 9(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Figured for Taxable Income for Current Tax Year for 1st 11 months divided by Average of Taxable Income Ratios for 1st 11 months of Tax Year #1 through Tax Year #3 -->
			<xsd:element name="EntireYearTaxFigured" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Figure the tax on the amount on line 9 using the instructions for Form 1120.</Description>
						<LineNumber>Schedule A Line 10(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 14(d), use value of 10(d) -->

			<!-- Alternative Minimum Tax for Entirety of Current Tax Year -->
			<xsd:element name="EntireYearAlternativeMinTax" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter alternative minimum tax for each payment period</Description>
						<LineNumber>Schedule A Line 15(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Taxes for Entirety of Current Tax Year -->
			<xsd:element name="EntireYearOtherTaxes" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter other taxes for each payment period</Description>
						<LineNumber>Schedule A Line 16(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total tax for Entirety of Current Tax Year -->
			<xsd:element name="EntireYearTotalTax" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax.  Add lines 14 through 16</Description>
						<LineNumber>Schedule A Line 17(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Credits for Entirety of Current Tax Year -->
			<xsd:element name="EntireYearTaxCredits" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>For each period, enter the same type of credits as allowed on Form 2220, lines 4 and 5c</Description>
						<LineNumber>Schedule A Line 18(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax After Credits for Entirety of Current Tax Year -->
			<xsd:element name="EntireYearTaxAfterCredits" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax after credits</Description>
						<LineNumber>Schedule A Line 19(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Part II - Annualized Income Installment Method ===== -->

			<!-- Annualization Period #1 - First ? months-->
			<xsd:element name="AnnualizationPeriod1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter annualization periods.  Choose from Option 1, Option 2, or Standard Option</Description>
						<LineNumber>Schedule A Line 20a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="IntegerNNType">
                        <xsd:totalDigits value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
			</xsd:element>

			<!-- Taxable Income for Annualization Period #1 -->
			<xsd:element name="TaxableIncome1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for annualization period #1</Description>
						<LineNumber>Schedule A Line 21a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Amount for Annualization Period #1 -->
			<xsd:element name="AnnualizationAmount1" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualization amount for annualization period #1</Description>
						<LineNumber>Schedule A Line 22a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="AnnualizationAmountType">
						<xsd:enumeration value="4" />		<!-- Standard Option or Option 2 -->
						<xsd:enumeration value="6" />		<!-- Option 1 -->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Annualized Taxable Income for Annualization Period #1 -->
			<xsd:element name="AnnualizedTaxableIncome1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualized taxable income for annualization period #1</Description>
						<LineNumber>Schedule A Line 23a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Tax Amount for Annualization Period #1 -->
			<xsd:element name="FiguredTaxAmount1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Figure the tax on the amount in each column on line 23 using the instructions for Form 1120.</Description>
						<LineNumber>Schedule A Line 24a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Alternative Minimum Tax for Annualization Period #1 -->
			<xsd:element name="AlternativeMinimumTax1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Alternative minimum tax for annualization period #1</Description>
						<LineNumber>Schedule A Line 25a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Taxes for Annualization Period #1 -->
			<xsd:element name="OtherTaxes1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other taxes for annualization period #1</Description>
						<LineNumber>Schedule A Line 26a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax for Annualization Period #1 -->
			<xsd:element name="TotalTax1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax.  Add lines 24 through 26</Description>
						<LineNumber>Schedule A Line 27a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Credits for Annualization Period #1 -->
			<xsd:element name="Credits1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>For each period, enter the same type of credits as allowed on Form 2220, lines 4 and 5c</Description>
						<LineNumber>Schedule A Line 28a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax After Credits for Annualization Period #1 -->
			<xsd:element name="TotalTaxAfterCredits1" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax after credits</Description>
						<LineNumber>Schedule A Line 29a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Product of Total Tax After Credits and Applicable Percentage for Annualization Period #1 -->
			<xsd:element name="TotalTaxAfterPercentage1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Product of total tax after credits and applicable percentage for annualization period #1</Description>
						<LineNumber>Schedule A Line 31a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Period #2 - First ? months-->
			<xsd:element name="AnnualizationPeriod2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter annualization periods.  Choose from Option 1, Option 2, or Standard Option</Description>
						<LineNumber>Schedule A Line 20b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="IntegerNNType">
                        <xsd:totalDigits value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
			</xsd:element>

			<!-- Taxable Income for Annualization Period #2 -->
			<xsd:element name="TaxableIncome2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for annualization period #2</Description>
						<LineNumber>Schedule A Line 21b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Amount for Annualization Period #2 -->
			<xsd:element name="AnnualizationAmount2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualization amount for annualization period #2</Description>
						<LineNumber>Schedule A Line 22b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="AnnualizationAmountType">
						<xsd:enumeration value="4" />		<!-- Standard Option -->
						<xsd:enumeration value="3" />		<!-- Option 1 -->
						<xsd:enumeration value="2.4" />		<!-- Option 2 -->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Annualized Taxable Income for Annualization Period #2 -->
			<xsd:element name="AnnualizedTaxableIncome2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualized taxable income for annualization period #2</Description>
						<LineNumber>Schedule A Line 23b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Tax Amount for Annualization Period #2 -->
			<xsd:element name="FiguredTaxAmount2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Figure the tax on the amount in each column on line 23 using the instructions for Form 1120.</Description>
						<LineNumber>Schedule A Line 24b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Alternative Minimum Tax for Annualization Period #2 -->
			<xsd:element name="AlternativeMinimumTax2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Alternative minimum tax for annualization period #2</Description>
						<LineNumber>Schedule A Line 25b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Taxes for Annualization Period #2 -->
			<xsd:element name="OtherTaxes2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other taxes for annualization period #2</Description>
						<LineNumber>Schedule A Line 26b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax for Annualization Period #2 -->
			<xsd:element name="TotalTax2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax.  Add lines 24 through 26</Description>
						<LineNumber>Schedule A Line 27b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Credits for Annualization Period #2 -->
			<xsd:element name="Credits2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>For each period, enter the same type of credits as allowed on Form 2220, lines 4 and 5c</Description>
						<LineNumber>Schedule A Line 28b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax After Credits for Annualization Period #2 -->
			<xsd:element name="TotalTaxAfterCredits2" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax after credits</Description>
						<LineNumber>Schedule A Line 29b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Product of Total Tax After Credits and Applicable Percentage for Annualization Period #2 -->
			<xsd:element name="TotalTaxAfterPercentage2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Product of total tax after credits and applicable percentage for annualization period #2</Description>
						<LineNumber>Schedule A Line 31b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Period #3 - First ? months-->
			<xsd:element name="AnnualizationPeriod3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter annualization periods.  Choose from Option 1, Option 2, or Standard Option</Description>
						<LineNumber>Schedule A Line 20c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="IntegerNNType">
                        <xsd:totalDigits value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
			</xsd:element>

			<!-- Taxable Income for Annualization Period #3 -->
			<xsd:element name="TaxableIncome3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for annualization period #3</Description>
						<LineNumber>Schedule A Line 21c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Amount for Annualization Period #3 -->
			<xsd:element name="AnnualizationAmount3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualization amount for annualization period #3</Description>
						<LineNumber>Schedule A Line 22c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="AnnualizationAmountType">
						<xsd:enumeration value="2" />			<!-- Standard Option -->
						<xsd:enumeration value="1.71429" />		<!-- Option 1 -->
						<xsd:enumeration value="1.5" />			<!-- Option 2 -->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Annualized Taxable Income for Annualization Period #3 -->
			<xsd:element name="AnnualizedTaxableIncome3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualized taxable income for annualization period #3</Description>
						<LineNumber>Schedule A Line 23c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Tax Amount for Annualization Period #3 -->
			<xsd:element name="FiguredTaxAmount3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Figure the tax on the amount in each column on line 23 using the instructions for Form 1120.</Description>
						<LineNumber>Schedule A Line 24c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Alternative Minimum Tax for Annualization Period #3 -->
			<xsd:element name="AlternativeMinimumTax3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Alternative minimum tax for annualization period #3</Description>
						<LineNumber>Schedule A Line 25c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Taxes for Annualization Period #3 -->
			<xsd:element name="OtherTaxes3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other taxes for annualization period #3</Description>
						<LineNumber>Schedule A Line 26c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax for Annualization Period #3 -->
			<xsd:element name="TotalTax3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax.  Add lines 24 through 26</Description>
						<LineNumber>Schedule A Line 27c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Credits for Annualization Period #3 -->
			<xsd:element name="Credits3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>For each period, enter the same type of credits as allowed on Form 2220, lines 4 and 5c</Description>
						<LineNumber>Schedule A Line 28c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax After Credits for Annualization Period #3 -->
			<xsd:element name="TotalTaxAfterCredits3" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax after credits</Description>
						<LineNumber>Schedule A Line 29c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Product of Total Tax After Credits and Applicable Percentage for Annualization Period #3 -->
			<xsd:element name="TotalTaxAfterPercentage3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Product of total tax after credits and applicable percentage for annualization period #3</Description>
						<LineNumber>Schedule A Line 31c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Period #4 - First ? months-->
			<xsd:element name="AnnualizationPeriod4" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter annualization periods.  Choose from Option 1, Option 2, or Standard Option</Description>
						<LineNumber>Schedule A Line 20d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
                <xsd:simpleType>
                    <xsd:restriction base="IntegerNNType">
                        <xsd:totalDigits value="2"/>
                    </xsd:restriction>
                </xsd:simpleType>
			</xsd:element>

			<!-- Taxable Income for Annualization Period #4 -->
			<xsd:element name="TaxableIncome4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income for annualization period #4</Description>
						<LineNumber>Schedule A Line 21d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Annualization Amount for Annualization Period #4 -->
			<xsd:element name="AnnualizationAmount4" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualization amount for annualization period #4</Description>
						<LineNumber>Schedule A Line 22d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="AnnualizationAmountType">
						<xsd:enumeration value="1.33333" />		<!-- Standard Option -->
						<xsd:enumeration value="1.2" />			<!-- Option 1 -->
						<xsd:enumeration value="1.09091" />		<!-- Option 2 -->
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Annualized Taxable Income for Annualization Period #4 -->
			<xsd:element name="AnnualizedTaxableIncome4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Annualized taxable income for annualization period #4</Description>
						<LineNumber>Schedule A Line 23d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Tax Amount for Annualization Period #4 -->
			<xsd:element name="FiguredTaxAmount4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Figure the tax on the amount in each column on line 23 using the instructions for Form 1120.</Description>
						<LineNumber>Schedule A Line 24d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Alternative Minimum Tax for Annualization Period #4 -->
			<xsd:element name="AlternativeMinimumTax4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Alternative minimum tax for annualization period #4</Description>
						<LineNumber>Schedule A Line 25d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Taxes for Annualization Period #4 -->
			<xsd:element name="OtherTaxes4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other taxes for annualization period #4</Description>
						<LineNumber>Schedule A Line 26d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax for Annualization Period #4 -->
			<xsd:element name="TotalTax4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax.  Add lines 24 through 26</Description>
						<LineNumber>Schedule A Line 27d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Credits for Annualization Period #4 -->
			<xsd:element name="Credits4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>For each period, enter the same type of credits as allowed on Form 2220, lines 4 and 5c</Description>
						<LineNumber>Schedule A Line 28d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Tax After Credits for Annualization Period #4 -->
			<xsd:element name="TotalTaxAfterCredits4" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total tax after credits</Description>
						<LineNumber>Schedule A Line 29d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Product of Total Tax After Credits and Applicable Percentage for Annualization Period #4 -->
			<xsd:element name="TotalTaxPercentage4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Product of total tax after credits and applicable percentage for annualization period #4</Description>
						<LineNumber>Schedule A Line 31d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Part III - Required Installments ===== -->

			<!-- Smaller of Annualized Income Installment or Adjusted Seasonal Installment for 1st Required Installment -->
			<xsd:element name="AnnualizedOrAdjustedSeasonal1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If only one of the above parts is completed, enter the amount in each column from line 19 or line 31. If both parts are completed, enter the smaller of the amounts in each column from line 19 or line 31</Description>
						<LineNumber>Schedule A Line 32(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Adjusted Seasonal Installment or Annualized income for 1st 4 months of Current Tax Year -->
			<xsd:element name="First4MonthsAdjSsnlOrAnnlzd" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Adjusted seasonal installment or annualized income for 1st 4 months of current tax year</Description>
						<LineNumber>Schedule A Line 34(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- 25% of Smaller Tax for 1st Required Installment -->
			<xsd:element name="PortionOfSmallerTax1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter 25% of line 5 on page 1 of Form 2220 in each column.</Description>
						<LineNumber>Schedule A Line 35(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- 25% of Smaller Tax for 2nd Required Installment -->
			<xsd:element name="PortionOfSmallerTax2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter 25% of line 5 on page 1 of Form 2220 in each column.</Description>
						<LineNumber>Schedule A Line 35(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- 25% of Smaller Tax for 3rd Required Installment -->
			<xsd:element name="PortionOfSmallerTax3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter 25% of line 5 on page 1 of Form 2220 in each column.</Description>
						<LineNumber>Schedule A Line 35(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- 25% of Smaller Tax for 4th Required Installment -->
			<xsd:element name="PortionOfSmallerTax4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter 25% of line 5 on page 1 of Form 2220 in each column.</Description>
						<LineNumber>Schedule A Line 35(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 37(a), use value of 35(a) -->

			<!-- Required Installment #1 -->
			<xsd:element name="RequiredInstallment1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the smaller of line 34 or line 37 here and on page 1 of Form 2220, line 10</Description>
						<LineNumber>Schedule A Line 38(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Smaller of Annualized Income Installment or Adjusted Seasonal Installment for 2nd Required Installment -->
			<xsd:element name="AnnualizedOrAdjustedSeasonal2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If only one of the above parts is completed, enter the amount in each column from line 19 or line 31. If both parts are completed, enter the smaller of the amounts in each column from line 19 or line 31</Description>
						<LineNumber>Schedule A Line 32(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of All Preceding Required Installments for Annualization Period #2 -->
			<xsd:element name="TotalPrecedingRequired2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of all preceding required installments for annualization period #2</Description>
						<LineNumber>Schedule A Line 33(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Adjusted Seasonal Installment or Annualized income for 1st 6 months of Current Tax Year -->
			<xsd:element name="First6MonthsAdjSsnlOrAnnlzd" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Adjusted seasonal installment or annualized income for 1st 6 months of current tax year</Description>
						<LineNumber>Schedule A Line 34(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 35(b), use value of 35(a), since the same value is to be used across all columns -->

			<!-- Installment Carryover from Required Installment #1 -->
			<xsd:element name="InstallmentCarryover2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Installment carryover from 1st required installment</Description>
						<LineNumber>Schedule A Line 36(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Required Installment by adding 25% of Smaller Tax for 2nd Required Installment and Installment Carryover -->
			<xsd:element name="FiguredRequiredInstallment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 35 and 36</Description>
						<LineNumber>Schedule A Line 37(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment #2 -->
			<xsd:element name="RequiredInstallment2" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the smaller of line 34 or line 37 here and on page 1 of Form 2220, line 10</Description>
						<LineNumber>Schedule A Line 38(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Smaller of Annualized Income Installment or Adjusted Seasonal Installment for 3rd Required Installment -->
			<xsd:element name="AnnualizedOrAdjustedSeasonal3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If only one of the above parts is completed, enter the amount in each column from line 19 or line 31. If both parts are completed, enter the smaller of the amounts in each column from line 19 or line 31</Description>
						<LineNumber>Schedule A Line 32(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of All Preceding Required Installments for Annualization Period #3 -->
			<xsd:element name="TotalPrecedingRequired3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of all preceding required installments for annualization period #3</Description>
						<LineNumber>Schedule A Line 33(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Adjusted Seasonal Installment or Annualized income for 1st 9 months of Current Tax Year -->
			<xsd:element name="First9MonthsAdjSsnlOrAnnlzd" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Adjusted seasonal installment or Annualized income for 1st 9 months of current tax year</Description>
						<LineNumber>Schedule A Line 34(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 35(c), use value of 35(a), since the same value is to be used across all columns -->

			<!-- Installment Carryover from Required Installment #2 -->
			<xsd:element name="InstallmentCarryover3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Installment carryover from 2nd required installment</Description>
						<LineNumber>Schedule A Line 36(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Required Installment by adding 25% of Smaller Tax for 3rd Required Installment and Installment Carryover -->
			<xsd:element name="FiguredRequiredInstallment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 35 and 36</Description>
						<LineNumber>Schedule A Line 37(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment #3 -->
			<xsd:element name="RequiredInstallment3" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the smaller of line 34 or line 37 here and on page 1 of Form 2220, line 10</Description>
						<LineNumber>Schedule A Line 38(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Smaller of Annualized Income Installment or Adjusted Seasonal Installment for 4th Required Installment -->
			<xsd:element name="AnnualizedOrAdjustedSeasonal4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If only one of the above parts is completed, enter the amount in each column from line 19 or line 31. If both parts are completed, enter the smaller of the amounts in each column from line 19 or line 31</Description>
						<LineNumber>Schedule A Line 32(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of All Preceding Required Installments for Annualization Period #4 -->
			<xsd:element name="TotalPrecedingRequired4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of all preceding required installments for annualization period #4</Description>
						<LineNumber>Schedule A Line 33d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Adjusted Seasonal Installment or Annualized income -->
			<xsd:element name="AdjustedSeasonalOrAnnualized" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Adjusted seasonal installment or annualized income</Description>
						<LineNumber>Schedule A Line 34(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- For 35(d), use value of 35(a), since the same value is to be used across all columns -->

			<!-- Installment Carryover from Required Installment #3 -->
			<xsd:element name="InstallmentCarryover4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Installment carryover from 3rd required installment</Description>
						<LineNumber>Schedule A Line 36(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Figured Required Installment by adding 25% of Smaller Tax for 4th Required Installment and Installment Carryover -->
			<xsd:element name="FiguredRequiredInstallment4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Add lines 35 and 36</Description>
						<LineNumber>Schedule A Line 37(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required Installment #4 -->
			<xsd:element name="RequiredInstallment4" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the smaller of line 34 or line 37 here and on page 1 of Form 2220, line 10</Description>
						<LineNumber>Schedule A Line 38(d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>

	</xsd:complexType>

	<!-- ===== Type for Payment Penalty Computation ===== -->

	<xsd:complexType name="PaymentPenaltyComputationType">

		<xsd:sequence>

			<!-- Payment Date -->
			<xsd:element name="PaymentDate" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the date of payment or the 15th day of the 3rd month after the close of the tax year, whichever is earlier</Description>
						<LineNumber>19a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date -->
			<xsd:element name="DaysFromDueDateToPaymentDate" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days from due date to payment date</Description>
						<LineNumber>20a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date within 1st Date Range -->
			<xsd:element name="LateDaysIn1stDateRange" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days on line 20</Description>
						<LineNumber>21a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty for Late Days within 1st Date Range for Payment Date -->
			<xsd:element name="PenaltyFor1stDateRange" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment on line 17</Description>
						<LineNumber>22a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date within 2nd Date Range -->
			<xsd:element name="LateDaysIn2ndDateRange" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days on line 20</Description>
						<LineNumber>23a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty for Late Days within 2nd Date Range for Payment Date -->
			<xsd:element name="PenaltyFor2ndDateRange" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment on line 17</Description>
						<LineNumber>24a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date within 3rd Date Range -->
			<xsd:element name="LateDaysIn3rdDateRange" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days on line 20</Description>
						<LineNumber>25a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty for Late Days within 3rd Date Range for Payment Date -->
			<xsd:element name="PenaltyFor3rdDateRange" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment on line 17</Description>
						<LineNumber>26a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date within 4th Date Range -->
			<xsd:element name="LateDaysIn4thDateRange" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days on line 20</Description>
						<LineNumber>27a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty for Late Days within 4th Date Range for Payment Date -->
			<xsd:element name="PenaltyFor4thDateRange" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment on line 17</Description>
						<LineNumber>28a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date within 5th Date Range -->
			<xsd:element name="LateDaysIn5thDateRange" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days on line 20</Description>
						<LineNumber>29a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty for Late Days within 5th Date Range for Payment Date -->
			<xsd:element name="PenaltyFor5thDateRange" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment on line 17</Description>
						<LineNumber>30a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Days from Due Date to Payment Date within 6th Date Range -->
			<xsd:element name="LateDaysIn6thDateRange" type="NumberOfDaysType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of days on line 20</Description>
						<LineNumber>31a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Penalty for Late Days within 6th Date Range for Payment Date -->
			<xsd:element name="PenaltyFor6thDateRange" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Underpayment on line 17</Description>
						<LineNumber>32a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Penalty for Payment Date -->
			<xsd:element name="TotalPenaltyForPaymentDate" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total penalty for payment date</Description>
						<LineNumber>33a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>

	</xsd:complexType>

	<!-- Number of Days Type -->
	<xsd:simpleType name="NumberOfDaysType">
		<xsd:annotation>
			<xsd:documentation>Number of days</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="IntegerType">
			<xsd:totalDigits value="3"/>
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Annualization Amount Type -->
	<xsd:simpleType name="AnnualizationAmountType">
		<xsd:annotation>
			<xsd:documentation>A decimal amount that allows up to 6 total digits and 5 fraction digits</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="6" />
			<xsd:fractionDigits value="5" />
		</xsd:restriction>
	</xsd:simpleType>

</xsd:schema>
