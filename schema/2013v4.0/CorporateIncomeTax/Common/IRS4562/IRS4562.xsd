<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.irs.gov/efile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.irs.gov/efile" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">

	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - Form4562</Description>
			<TaxYear>2013</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>July 18, 2014</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>

	<!-- =============================================================== -->
	<!-- =======================  IRS Form 4562 ======================== -->
	<!-- =============================================================== -->

	<xsd:element name="IRS4562">
		<xsd:annotation>
			<xsd:documentation>IRS Form 4562</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS4562Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS4562">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>

					<xsd:attribute name="referenceDocumentId" type="IdListType">
						<xsd:annotation>
							<xsd:documentation>List of document ID's of forms, schedules, supporting info. etc. attached to this form</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>

					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="GeneralDependencySmall"/>

					<!-- Half-year Convention Election (optional label) -->
					<xsd:attribute name="electionPursuantNoticeCd">
						<xsd:simpleType>
							<xsd:restriction base="StringType">
								<xsd:enumeration value="ELECTION PURSUANT TO NOTICE 2001-70"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<!-- Extension Under Announcement (optional label) -->
					<xsd:attribute name="extnsUnderAnnouncementCd">
						<xsd:simpleType>
							<xsd:restriction base="StringType">
								<xsd:enumeration value="EXTENSION UNDER ANNOUNCEMENT 2006-29"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS4562Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 4562</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- Business or Activity -->
			<xsd:element name="BusinessOrActivityTxt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Business or activity to which this form relates</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="TextType">
						<xsd:maxLength value="40"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- ===== Part I - Election To Expense Certain Property Under Section 179 ===== -->

			<!-- Maximum Dollar Limitation -->
			<xsd:element name="MaximumDollarLimitationAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Maximum dollar limitation</Description>
						<LineNumber>1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Cost of Section 179 Property placed in service -->
			<xsd:element name="TotalCostOfSection179PropAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total cost of section 179 property placed in service</Description>
						<LineNumber>2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Threshold Cost of Section 179 Property -->
			<xsd:element name="ThresholdCostOfSect179PropAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Threshold cost of section 179 property</Description>
						<LineNumber>3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Reduction in Limitation -->
			<xsd:element name="ReductionInLimitationAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Reduction in limitation</Description>
						<LineNumber>4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Dollar Limitation for Tax Year -->
			<xsd:element name="DollarLimitationForTaxYearAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Dollar limitation for tax year</Description>
						<LineNumber>5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Elected Property Table -->
			<xsd:element name="ElectedProperty" type="ElectedPropertyType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Elected property table</Description>
						<LineNumber>6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Line 7 - Listed Property.  Copy of line 29 -->

			<!-- Total Elected Cost of Section 179 Property placed in service -->
			<xsd:element name="TotalElectedCostSect179PropAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total elected cost of section 179 property placed in service</Description>
						<LineNumber>8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tentative Deduction -->
			<xsd:element name="TentativeDeductionAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tentative deduction</Description>
						<LineNumber>9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Prior Year Carryover of Disallowed Deduction -->
			<xsd:element name="DisallowedDeductionCyovAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Prior year carryover of disallowed deduction</Description>
						<LineNumber>10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Business Income Limitation -->
			<xsd:element name="BusinessIncomeLimitationAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Business income limitation</Description>
						<LineNumber>11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Section 179 Expense Deduction -->
			<xsd:element name="Section179ExpenseDeductionAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 179 expense deduction</Description>
						<LineNumber>12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="section179ExpnsDedSummaryCd">
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:enumeration value="SUMMARY"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Next Year Carryover Amount -->
			<xsd:element name="NextYearCarryoverAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Next year carryover amount</Description>
						<LineNumber>13</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Part II - Special Depreciation Allowance and Other Depreciation ===== -->

			<!-- Special Allowance -->
			<xsd:element name="SpecialAllowanceAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Special allowance</Description>
						<LineNumber>Part II Line 14</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Property subject to section 168(f)(1) election -->
			<xsd:element name="Section168f1ElectedPropertyAmt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Property subject to section 168(f)(1) election</Description>
						<LineNumber>Part II Line 15</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="Section168f1PropertyExplanationStatement"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Other depreciation  -->
			<xsd:element name="OtherDepreciationAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other depreciation</Description>
						<LineNumber>Part II Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- ===== Part III - MACRS Depreciation ===== -->

			<!-- MACRS Deductions for assets placed in service before CY -->
			<xsd:element name="MACRSDedForAstInSrvcBfrPYAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>MACRS Deductions for assets placed in service before CY</Description>
						<LineNumber>17</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- General Asset Account Election -->
			<xsd:element name="GeneralAssetAccountElectionInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>General asset account election</Description>
						<LineNumber>18</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- General Depreciation System -->
			<xsd:element name="GeneralDepreciationSystem" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>General Depreciation System</Description>
						<LineNumber>19</LineNumber>
					</xsd:documentation>
				</xsd:annotation>

				<xsd:complexType>
					<xsd:sequence>

						<!-- General Depreciation System (GDS) for 3-year Property -->
						<xsd:element name="GDS3YearProperty" type="GDS3To20YearPropertyType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 3-year property</Description>
									<LineNumber>19a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- General Depreciation System (GDS) for 5-year Property -->
						<xsd:element name="GDS5YearProperty" type="GDS3To20YearPropertyType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 5-year property</Description>
									<LineNumber>19b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- General Depreciation System (GDS) for 7-year Property -->
						<xsd:element name="GDS7YearProperty" type="GDS3To20YearPropertyType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 7-year property</Description>
									<LineNumber>19c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- General Depreciation System (GDS) for 10-year Property -->
						<xsd:element name="GDS10YearProperty" type="GDS3To20YearPropertyType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 10-year property</Description>
									<LineNumber>19d</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- General Depreciation System (GDS) for 15-year Property -->
						<xsd:element name="GDS15YearProperty" type="GDS3To20YearPropertyType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 15-year property</Description>
									<LineNumber>19e</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- General Depreciation System (GDS) for 20-year Property -->
						<xsd:element name="GDS20YearProperty" type="GDS3To20YearPropertyType" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 20-year property</Description>
									<LineNumber>19f</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- General Depreciation System (GDS) for 25-year Property -->
						<xsd:element name="GDS25YearProperty" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for 25-year property</Description>
									<LineNumber>19g</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<!-- Basis for Depreciation -->
									<xsd:element name="BasisForDepreciationAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Basis for depreciation</Description>
												<LineNumber>19g column c</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Convention -->
									<xsd:element name="DepreciationConventionCd" type="DepreciationConventionCodeType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Convention</Description>
												<LineNumber>19g column e</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Depreciation Deduction -->
									<xsd:element name="DepreciationDeductionAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Depreciation deduction</Description>
												<LineNumber>19g column g</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>

						<!-- General Depreciation System (GDS) for Residential Rental Property -->
						<xsd:element name="GDSResidentialRentalProperty" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for residential rental property</Description>
									<LineNumber>19h</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<!-- Month and Year Placed in Service -->
									<xsd:element name="MonthAndYearPlacedInServiceDt" type="YearMonthType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Month and year placed in service</Description>
												<LineNumber>19h column b</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Basis for Depreciation -->
									<xsd:element name="BasisForDepreciationAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Basis for depreciation</Description>
												<LineNumber>19h column c</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Depreciation Deduction -->
									<xsd:element name="DepreciationDeductionAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Depreciation deduction</Description>
												<LineNumber>19h column g</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>

						<!-- General Depreciation System (GDS) for Non-Residential Real Property (type 1: 39 yr Recovery Period) -->
						<xsd:element name="GDSNonRsdntlRealProp" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for non-residential real property (type 1)</Description>
									<LineNumber>19i(1)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<!-- Month and Year Placed in Service -->
									<xsd:element name="MonthAndYearPlacedInServiceDt" type="YearMonthType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Month and year placed in service</Description>
												<LineNumber>19i(1) column b</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Basis for Depreciation -->
									<xsd:element name="BasisForDepreciationAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Basis for depreciation</Description>
												<LineNumber>19i(1) column c</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Depreciation Deduction -->
									<xsd:element name="DepreciationDeductionAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Depreciation deduction</Description>
												<LineNumber>19i(1) column g</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>

						<!-- General Depreciation System (GDS) for Non-Residential Real Property (type 2: specify Recovery Period) -->
						<xsd:element name="GDSNonRsdntlRealPropSpecify" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>GDS table for non-residential real property (type 2)</Description>
									<LineNumber>19i(2)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<!-- Month and Year Placed in Service -->
									<xsd:element name="MonthAndYearPlacedInServiceDt" type="YearMonthType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Month and year placed in service</Description>
												<LineNumber>19i(2) column b</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Basis for Depreciation -->
									<xsd:element name="BasisForDepreciationAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Basis for depreciation</Description>
												<LineNumber>19i(2) column c</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Recovery Period -->
									<xsd:element name="RecoveryPrd" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Recovery period</Description>
												<LineNumber>19i(2) column d</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="DecimalType">
												<xsd:totalDigits value="3"/>
												<xsd:fractionDigits value="1"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<!-- Depreciation Deduction -->
									<xsd:element name="DepreciationDeductionAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Depreciation deduction</Description>
												<LineNumber>19i(2) column g</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>

					</xsd:sequence>

					<xsd:attribute name="referenceDocumentId" type="IdListType"/>
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="50YearGDSDeductionStatement"/>

				</xsd:complexType>
			</xsd:element>

			<!-- Alternative Depreciation System -->
			<xsd:element name="AlternativeDepreciationSystem" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Alternative Depreciation System</Description>
						<LineNumber>20</LineNumber>
					</xsd:documentation>
				</xsd:annotation>

				<xsd:complexType>
					<xsd:sequence>

						<!-- Alternative Depreciation System (ADS) for Class Life Property -->
						<xsd:element name="ADSClassLifeProperty" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>ADS table for class life property</Description>
									<LineNumber>20a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<!-- Basis for Depreciation -->
									<xsd:element name="BasisForDepreciationAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Basis for depreciation</Description>
												<LineNumber>20a column c</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Recovery Period -->
									<xsd:element name="RecoveryPrd" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Recovery period</Description>
												<LineNumber>20a column d</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="DecimalType">
												<xsd:totalDigits value="3"/>
												<xsd:fractionDigits value="1"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<!-- Convention -->
									<xsd:element name="DepreciationConventionCd" type="DepreciationConventionCodeType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Convention</Description>
												<LineNumber>20a column e</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Depreciation Deduction -->
									<xsd:element name="DepreciationDeductionAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Depreciation deduction</Description>
												<LineNumber>20a column g</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>

						<!-- Alternative Depreciation System (ADS) for 12-year Property -->
						<xsd:element name="ADS12YearProperty" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>ADS table for 12-year property</Description>
									<LineNumber>20b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<!-- Basis for Depreciation -->
									<xsd:element name="BasisForDepreciationAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Basis for depreciation</Description>
												<LineNumber>20b column c</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Convention -->
									<xsd:element name="DepreciationConventionCd" type="DepreciationConventionCodeType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Convention</Description>
												<LineNumber>20b column e</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<!-- Depreciation Deduction -->
									<xsd:element name="DepreciationDeductionAmt" type="USAmountType" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Depreciation deduction</Description>
												<LineNumber>20b column g</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
