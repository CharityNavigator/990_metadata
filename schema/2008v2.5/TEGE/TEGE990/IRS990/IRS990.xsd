						<Description>Investments other securities</Description>
						<LineNumber>Part X Line 12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investments-Program Related -->
			<xsd:element name="InvestmentsProgramRelated" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investments-program related</Description>
						<LineNumber>Part X Line 13</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>			

			<!-- Intangible Assets -->
			<xsd:element name="IntangibleAssets" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Intangible assets</Description>
						<LineNumber>Part X Line 14</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>			

			<!-- Other Assets -->
			<xsd:element name="OtherAssetsTotal" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other assets</Description>
						<LineNumber>Part X Line 15</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Total Assets -->
			<xsd:element name="TotalAssets" type="Form990PartXGroup2Type" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total assets</Description>
						<LineNumber>Part X Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part X Balance Sheet -->			
			<!-- Liabilities Section -->			
			<!-- Accounts Payable and Accrued Expenses -->
			<xsd:element name="AccountsPayableAccruedExpenses" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Accounts payable and accrued expenses</Description>
						<LineNumber>Part X Line 17</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grants Payable -->
			<xsd:element name="GrantsPayable" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants payable</Description>
						<LineNumber>Part X Line 18</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Deferred Revenue -->
			<xsd:element name="DeferredRevenue" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Deferred revenue</Description>
						<LineNumber>Part X Line 19</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Tax-Exempt Bond Liabilities -->
			<xsd:element name="TaxExemptBondLiabilities" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax-exempt bond liabilities</Description>
						<LineNumber>Part X Line 20</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Escrow Account Liability -->
			<xsd:element name="EscrowAccountLiability" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Escrow account liability</Description>
						<LineNumber>Part X Line 21</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Loans From Officers, Directors, Trustees or Key Employees -->
			<xsd:element name="LoansFromOfficersDirectors" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Payable to current and former officers, directors, trustees, or key employees</Description>
						<LineNumber>Part X Line 22</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Mortgages and Notes Payable Secured by Investment Property -->
			<xsd:element name="MortNotesPyblSecuredInvestProp" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Mortgages and notes payable secured by investment property</Description>
						<LineNumber>Part X Line 23</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Unsecured Notes and Loans Payable -->
			<xsd:element name="UnsecuredNotesLoansPayable" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Unsecured notes and loans payable</Description>
						<LineNumber>Part X Line 24</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Other Liabilities -->
			<xsd:element name="OtherLiabilities" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other liabilities</Description>
						<LineNumber>Part X Line 25</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Total Liabilities -->
			<xsd:element name="TotalLiabilities" type="Form990PartXGroup2Type" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total liabilities</Description>
						<LineNumber>Part X Line 26</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Part X Balance Sheet -->	
			<!-- Net Assets or Fund Balances Section -->				

			<!-- Organizations That Follow SFAS 117, Check Here -->
			<xsd:element name="FollowSFAS117" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Organizations that follow SFAS 117, check here</Description>
						<LineNumber>Part X</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Unrestricted Net Assets -->
			<xsd:element name="UnrestrictedNetAssets" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Unrestricted net assets</Description>
						<LineNumber>Part X Line 27</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Temporarily Restricted Net Assets -->
			<xsd:element name="TemporarilyRestrictedNetAssets" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Temporarily restricted net assets</Description>
						<LineNumber>Part X Line 28</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Permanently Restricted Net Assets -->
			<xsd:element name="PermanentlyRestrictedNetAssets" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Permanently restricted net assets</Description>
						<LineNumber>Part X Line 29</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Organizations That Do Not Follow SFAS 117, Check Here -->
			<xsd:element name="DoNotFollowSFAS117" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Organizations that do not follow SFAS 117, check here</Description>
						<LineNumber>Part X</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Capital Stock or Trust Principal, or Current Funds -->
			<xsd:element name="CapStckTrstPrinCurrentFunds" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital stock or trust principal, or current funds</Description>
						<LineNumber>Part X Line 30</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Paid-In or Capital Surplus, or Land, Building or Equipment Fund -->
			<xsd:element name="PaidInCapSrplsLandBldgEqpFund" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Paid-in or capital surplus, or land, building or equipment fund</Description>
						<LineNumber>Part X Line 31</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Retained Earnings, Endowment, Accumulated Income, or Other Funds -->
			<xsd:element name="RetainedEarningsEndowmentEtc" type="Form990PartXGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Retained earnings, endowment, accumulated income, or other funds</Description>
						<LineNumber>Part X Line 32</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Total Net Assets or Fund Balances -->
			<xsd:element name="TotalNetAssetsFundBalances" type="Form990PartXGroup2Type" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net assets or fund balances</Description>
						<LineNumber>Part X Line 33</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Total Liabilities and Net Assets/Fund Balances -->
			<xsd:element name="TotalLiabNetAssetsFundBalances" type="Form990PartXGroup2Type" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total liabilities and net assets or fund balances</Description>
						<LineNumber>Part X Line 34</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element>

			<!-- Part XI Financials --> 

			<!-- Method of Accounting -->
			<xsd:choice minOccurs="1">
				<!-- Cash -->
				<xsd:element name="MethodOfAccountingCash" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Method of accounting - Cash</Description>
							<LineNumber>Part XI Line 1</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Method of Accounting - Accrual -->
				<xsd:element name="MethodOfAccountingAccrual" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Method of accounting - Accrual</Description>
							<LineNumber>Part XI Line 1</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Method of Accounting - Other -->
				<xsd:element name="MethodOfAccountingOther" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Method of accounting - Other</Description>
							<LineNumber>Part XI Line 1</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Accountant Provide Compilation or Review? -->
			<xsd:element name="AccountantCompileOrReview" type="BooleanType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Accountant provide compilation or review?</Description>
						<LineNumber>Part XI Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Financial Sheets Audited? -->
			<xsd:element name="FSAudited" type="BooleanType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Financial sheets audited?</Description>
						<LineNumber>Part XI Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Audit Committee? -->
			<xsd:element name="AuditCommittee" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Does the organization have an audit committee?</Description>
						<LineNumber>Part XI Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Federal Grant Audit Required? -->
			<xsd:element name="FederalGrantAuditRequired" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Federal grant audit required?</Description>
						<LineNumber>Part XI Line 3a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Federal Grant Audit Performed? -->
			<xsd:element name="FederalGrantAuditPerformed" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Federal grant audit performed?</Description>
						<LineNumber>Part XI Line 3b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- =========================================================================== -->
	<!-- ============= Local types, types for repeating groups etc. ================ -->
