						<xsd:element name="Title" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Title</Description>
									<LineNumber>Part VII Section A Line 1a A</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

