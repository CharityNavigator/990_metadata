					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fundraising Events -->
			<xsd:element name="FundraisingEvents" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fundraising events</Description>
						<LineNumber>Part VIII Line 1c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

