	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleEType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 5471 Schedule E</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- US Income, War Profits, and Excess Profits Taxes Paid or Accrued -->
			<xsd:element name="USIncomeAndProfitsTaxes" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>US amount of tax</Description>
						<LineNumber>Schedule E Line 1, Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Income, War Profits, and Excess Profits Taxes Paid or Accrued -->

			<xsd:element name="IncomeAndWarProfitsTaxesPaid" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Income, war profits, and excess profits taxes paid or accrued</Description>
						<LineNumber>Schedule E Lines 2 through 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Name of Country or US Posession -->
						<xsd:element name="NameOfCountryOrUSPosession" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Name of country or US posession</Description>
									<LineNumber>Schedule E Line 2, Column (a)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="StringType">
									<xsd:maxLength value="35"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>

						<!-- Amount of Tax in Foreign Currency -->
						<xsd:element name="AmountOfTaxInForeignCurrency" type="ForeignAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of tax in foreign currency</Description>
									<LineNumber>Schedule E Line 2, Column (b)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Amount of Tax Conversion Rate -->
						<xsd:element name="AmountOfTaxConversionRate" type="LargeRatioType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of tax conversion rate</Description>
									<LineNumber>Schedule E Line 2, Column (c)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Amount of Tax in US Dollars -->
						<xsd:element name="AmountOfTaxInUSDollars" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of tax in US dollars</Description>
									<LineNumber>Schedule E Line 2, Column (d)</LineNumber>
								</xsd:documentation>
