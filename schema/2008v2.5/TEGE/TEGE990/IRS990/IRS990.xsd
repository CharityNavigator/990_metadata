	<!-- Type for Part III, Repeating Activities Lines 4b through 4d -->
	<xsd:complexType name="Form990PartIIIGroup1Type">
		<xsd:sequence>

			<!-- Activity Code -->
			<xsd:element name="ActivityCode" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Activity code</Description>
						<LineNumber>Part III</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Expense -->
			<xsd:element name="Expense" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Expense</Description>
						<LineNumber>Part III</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grants -->
			<xsd:element name="Grants" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants</Description>
						<LineNumber>Part III</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Revenue -->
			<xsd:element name="Revenue" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
