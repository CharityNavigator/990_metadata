			<xsd:element name="IncomeTaxes" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Income taxes</Description>
						<LineNumber>Schedule D Part III Section C Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Cost of Strikes -->
			<xsd:element name="CostOfStrikes" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Cost of strikes</Description>
						<LineNumber>Schedule D Part III Section C Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Warranty and Product Liability Costs -->
			<xsd:element name="WarrantyProductLiabilityCosts" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Warranty and product liability costs</Description>
						<LineNumber>Schedule D Part III Section C Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Section 179 Costs -->
			<xsd:element name="Section179Costs" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 179 costs</Description>
						<LineNumber>Schedule D Part III Section C Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- On-site Storage -->
			<xsd:element name="OnSiteStorage" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>On-site storage</Description>
						<LineNumber>Schedule D Part III Section C Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Depreciation, Amortization, and Cost Recovery Allowance -->
			<xsd:element name="OtherDeprecAmortzCostRecovery" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depreciation, amortization, and cost recovery allowance not included one line 11 above</Description>
						<LineNumber>Schedule D Part III Section C Line 10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Costs (NOT required to be allocated) -->
			<xsd:element name="OtherCostsNotRqrToBeAllocated" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other costs (Attach a list of these costs)</Description>
						<LineNumber>Schedule D Part III Section C Line 11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="PresentAndProposedMethodCheckboxesType">
							<xsd:attribute name="referenceDocumentId" type="IdListType" />
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="OtherCostsNotRequiredToBeAllocatedAttachment" />
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Inventory Identification and Valuation Method checkboxes in Schedule D, Part II -->
	<xsd:complexType name="InventoryIdentificationAndValuationMethodsType">
		<xsd:sequence>

			<!-- Specific Identification -->
			<xsd:element name="SpecificIdentification" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Specific identification</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- FIFO -->
			<xsd:element name="FIFO" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>FIFO</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- LIFO -->
			<xsd:element name="LIFO" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>LIFO</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Other -->
			<xsd:element name="OtherIdentificationMethod" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other (attach explanation)</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType" />
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="OtherIdentificationAndValuationMethodsStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Cost -->
			<xsd:element name="Cost" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Cost</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Cost or Market -->
			<xsd:element name="CostOrMarket" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Cost or market, whichever is lower</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Retail Cost -->
			<xsd:element name="RetailCost" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Retail cost</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Retail, Lower of Cost or Market -->
			<xsd:element name="RetailLowerOfCostOrMarket" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Retail, lower of cost or market</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other -->
			<xsd:element name="OtherValutionMethod" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other (attach explanation)</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType" />
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="OtherIdentificationAndValuationMethodsStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
