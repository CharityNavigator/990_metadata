<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.irs.gov/efile" xmlns="http://www.irs.gov/efile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Tax Exempt and Government Entities Schema - IRS Form 990</Description>
			<TaxYear>2008</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>December 12, 2008</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>
	
	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>	
	
	<!-- =============================================================== -->
	<!-- =========================  IRS Form 990 ======================= -->
	<!-- =============================================================== -->
	
	<xsd:element name="IRS990">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentId" type="IdListType" />
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS990ScheduleO ReasonableCauseExplanation"/>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<xsd:complexType name="IRS990Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Form 990 Header Information --> 
			<!-- Address Change Indicator -->
			<xsd:element name="AddressChange" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates this return has an address change</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Name Change Indicator (Paper filers only) -->
			<!--xsd:element name="NameChange" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates this return has a name change</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element-->

			<!-- Initial Return Indicator -->
			<xsd:element name="InitialReturn" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates this is an initial return</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Termination Indicator -->
			<xsd:element name="TerminationReturn" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates this return is a termination return</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!--Amended Return Indicator -->
			<xsd:element name="AmendedReturn" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates this return is an amended return</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Application Pending Indicator (Paper filers only) -->
			<!-- xsd:element name="ApplicationPending" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Indicates this return has an application pending</Description>
						<LineNumber>B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element-->

			<!-- Doing Business As -->
			<xsd:element name="DoingBusinessAs" type="BusinessNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Doing business as</Description>
						<LineNumber>C</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Name of Principal Officer -->
			<!-- Name Choice -->
			<xsd:choice minOccurs="0">
				<xsd:element name="NameOfPrincipalOfficerPerson" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Name of principal officer - Person</Description>
							<LineNumber>F</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="NameOfPrincipalOfficerBusiness" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Name of principal officer - Business</Description>
							<LineNumber>F</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Address of Principal Officer -->
			<!-- Address Choice -->
			<xsd:choice minOccurs="0">
				<xsd:element name="AddressPrincipalOfficerUS" type="USAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Address of principal officer - US</Description>
							<LineNumber>F</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="AddressPrincipalOfficerForeign" type="ForeignAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Address of principal officer - Foreign</Description>
							<LineNumber>F</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Gross Receipts -->
			<xsd:element name="GrossReceipts" type="USAmountNNType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gross receipts</Description>
						<LineNumber>G</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Group Return for Affiliates? -->
			<xsd:element name="GroupReturnForAffiliates" type="BooleanType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Group return for affiliates?</Description>
						<LineNumber>H(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- All Affiliates Included? -->
			<xsd:element name="AllAffiliatesIncluded" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>All affiliates included?</Description>
						<LineNumber>H(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="AffiliateListing"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Group Exemption Number -->
			<xsd:element name="GroupExemptionNumber" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Group exemption number</Description>
						<LineNumber>H(c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
						<xsd:pattern value="\d{4}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Organization Type (Check Only One) -->
			<xsd:choice>
				<!-- Is it a 501(c) Organization? -->
				<xsd:element name="Organization501c">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Indicates a 501(c) organization</Description>
							<LineNumber>I</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="CheckboxType">
								<xsd:attribute name="typeOf501cOrganization">
									<xsd:simpleType>
										<xsd:restriction base="StringType">
											<xsd:pattern value="[2-9]|1[0-9]|2[02-7]"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>

				<!-- Is it a 4947(a)(1) Organization? -->
				<xsd:element name="Organization4947a1" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Indicates a 4947(a)(1) organization</Description>
							<LineNumber>I</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Is it a 527 Organization? -->
				<xsd:element name="Organization527" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Indicates a 527 organization</Description>
							<LineNumber>I</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Web Site -->
			<xsd:element name="WebSite" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Web site</Description>
						<LineNumber>J</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Type of Organization - Corporation -->
			<xsd:element name="TypeOfOrganizationCorporation" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Type of organization - corporation</Description>
						<LineNumber>K</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Type of Organization - Trust -->
			<xsd:element name="TypeOfOrganizationTrust" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Type of organization - trust</Description>
						<LineNumber>K</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Type of Organization - Association -->
			<xsd:element name="TypeOfOrganizationAssociation" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Type of organization - association</Description>
						<LineNumber>K</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Type of Organization - Other -->
			<xsd:element name="TypeOfOrganizationOther" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Type of organization - other</Description>
						<LineNumber>K</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Type of Organization - Other Description -->
			<xsd:element name="TypeOfOrgOtherDescription" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Type of organization - other description</Description>
						<LineNumber>K</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Year of Formation -->
			<xsd:element name="YearFormation" type="YearType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Year of formation</Description>
						<LineNumber>L</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- State of Legal Domicile -->
			<xsd:choice minOccurs="0">
				<!-- State Code -->
				<xsd:element name="StateLegalDomicile" type="StateType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>State of legal domicile</Description>
						<LineNumber>M</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				</xsd:element>

				<!--  Country Code -->
				<xsd:element name="CountryLegalDomicile" type="CountryType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Country of legal domicile</Description>
						<LineNumber>M</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Part I Summary --> 

			<!-- Activity or Mission Description -->
			<xsd:element name="ActivityOrMissionDescription" type="ShortExplanationType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Activity or mission description</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Termination or Contraction -->
			<xsd:element name="TerminationOrContraction" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Termination or contraction</Description>
						<LineNumber>Part I Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number Voting Members Governing Body -->
			<xsd:element name="NbrVotingMembersGoverningBody" type="IntegerNNType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number voting members governing body</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number Independent Voting Members -->
			<xsd:element name="NbrIndependentVotingMembers" type="IntegerNNType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number independent voting members</Description>
						<LineNumber>Part I Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Number Employees -->
			<xsd:element name="TotalNbrEmployees" type="IntegerNNType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>total Number employees</Description>
						<LineNumber>Part I Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Number Volunteers -->
			<xsd:element name="TotalNbrVolunteers" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total number volunteers</Description>
						<LineNumber>Part I Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Gross UBI -->
			<xsd:element name="TotalGrossUBI" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total gross UBI</Description>
						<LineNumber>Part I Line 7a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Net Unrelated Business Taxable Income -->
			<xsd:element name="NetUnrelatedBusinessTxblIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net unrelated business taxable income</Description>
						<LineNumber>Part I Line 7b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Contributions and Grants - Prior Year -->
			<xsd:element name="ContributionsGrantsPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Contributions and grants - prior year</Description>
						<LineNumber>Part I Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Contributions and Grants - Current Year -->
			<xsd:element name="ContributionsGrantsCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Contributions and grants - current year</Description>
						<LineNumber>Part I Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Program Service Revenue - Prior Year -->
			<xsd:element name="ProgramServiceRevenuePriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Program service revenue - prior year</Description>
						<LineNumber>Part I Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Program Service Revenue - Current Year -->
			<xsd:element name="ProgramServiceRevenueCY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Program service revenue - current year</Description>
						<LineNumber>Part I Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investment Income - Prior Year -->
			<xsd:element name="InvestmentIncomePriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment income - prior year</Description>
						<LineNumber>Part I Line 10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investment Income - Current Year -->
			<xsd:element name="InvestmentIncomeCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment income - current year</Description>
						<LineNumber>Part I Line 10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Revenue - Prior Year -->
			<xsd:element name="OtherRevenuePriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other revenue - prior year</Description>
						<LineNumber>Part I Line 11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Revenue - Current Year -->
			<xsd:element name="OtherRevenueCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other revenue - current year</Description>
						<LineNumber>Part I Line 11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Revenue - Prior Year -->
			<xsd:element name="TotalRevenuePriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total revenue - prior year</Description>
						<LineNumber>Part I Line 12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Revenue - Current Year -->
			<xsd:element name="TotalRevenueCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total revenue - current year</Description>
						<LineNumber>Part I Line 12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grants and Similar Amounts - Prior Year -->
			<xsd:element name="GrantsAndSimilarAmntsPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants and similar amounts - prior year</Description>
						<LineNumber>Part I Line 13</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grants and Similar Amounts - Current Year -->
			<xsd:element name="GrantsAndSimilarAmntsCY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants and similar amounts - current year</Description>
						<LineNumber>Part I Line 13</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Benefits Paid to Members - Prior Year -->
			<xsd:element name="BenefitsPaidToMembersPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Benefits paid to members - prior year</Description>
						<LineNumber>Part I Line 14</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Benefits Paid to Members - Current Year -->
			<xsd:element name="BenefitsPaidToMembersCY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Benefits paid to members - current year</Description>
						<LineNumber>Part I Line 14</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Salaries, Etc - Prior Year -->
			<xsd:element name="SalariesEtcPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Salaries, etc - prior year</Description>
						<LineNumber>Part I Line 15</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Salaries, Etc - Current Year -->
			<xsd:element name="SalariesEtcCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Salaries, etc - current year</Description>
						<LineNumber>Part I Line 15</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Professional Fundraising Expense - Prior Year -->
			<xsd:element name="TotalProfFundrsngExpPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total professional fundraising expense - prior year</Description>
						<LineNumber>Part I Line 16a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Professional Fundraising Expense - Current Year -->
			<xsd:element name="TotalProfFundrsngExpCY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total professional fundraising expense - current year</Description>
						<LineNumber>Part I Line 16a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Fundraising Expense - Current Year -->
			<xsd:element name="TotalFundrsngExpCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total fundraising expense - current year</Description>
						<LineNumber>Part I Line 16b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Expenses - Prior Year -->
			<xsd:element name="OtherExpensePriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other expenses - prior year</Description>
						<LineNumber>Part I Line 17</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Expenses - Current Year -->
			<xsd:element name="OtherExpensesCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other expenses - current year</Description>
						<LineNumber>Part I Line 17</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Expenses - Prior Year -->
			<xsd:element name="TotalExpensesPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Expenses - prior year</Description>
						<LineNumber>Part I Line 18</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Expenses - Current Year  -->
			<xsd:element name="TotalExpensesCurrentYear" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Expenses - current year </Description>
						<LineNumber>Part I Line 18</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Revenues Less Expenses - Prior Year -->
			<xsd:element name="RevenuesLessExpensesPriorYear" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Revenues less expenses - prior year</Description>
						<LineNumber>Part I Line 19</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Revenues Less Expenses - Current Year -->
			<xsd:element name="RevenuesLessExpensesCY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Revenues less expenses - current year</Description>
						<LineNumber>Part I Line 19</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Assets, BOY -->
			<xsd:element name="TotalAssetsBOY" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Assets, BOY</Description>
						<LineNumber>Part I Line 20</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Assets, EOY -->
			<xsd:element name="TotalAssetsEOY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Assets, EOY</Description>
