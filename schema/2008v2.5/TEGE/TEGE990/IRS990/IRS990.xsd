						<LineNumber>Part VIII Column (D)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		
		</xsd:sequence>
	</xsd:complexType>

	<!-- Group for Part IX, First Two Columns, All Optional -->
	<xsd:group name="Form990PartIXGroup1">
		<xsd:sequence>
		
			<!-- Total -->
			<xsd:element name="Total" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total</Description>
						<LineNumber>col (A)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Program Services -->
			<xsd:element name="ProgramServices" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Program services</Description>
						<LineNumber>col (B)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>


	<!-- Group for Part IX, Last Two Columns, All Optional -->
	<xsd:group name="Form990PartIXGroup2">
		<xsd:sequence>
		
			<!-- Management and General -->
			<xsd:element name="ManagementAndGeneral" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Management and general</Description>
						<LineNumber>col (C)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Fundraising -->
			<xsd:element name="Fundraising" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fundraising</Description>
						<LineNumber>col (D)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>

	<!-- Group for Part IX, Columns (A) and (B) -->
	<xsd:complexType name="Form990PartIXGroup1Type">
		<xsd:sequence>
			<xsd:group ref="Form990PartIXGroup1" />
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Group for Part IX, All Columns (A) - (D) -->
	<xsd:complexType name="Form990PartIXGroup2Type">
		<xsd:sequence>
		
			<xsd:group ref="Form990PartIXGroup1" />
			<xsd:group ref="Form990PartIXGroup2" />			

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Group for Part IX, Columns (A) - (D), Column (A) Required -->
	<xsd:complexType name="Form990PartIXGroup3Type">
		<xsd:sequence>
		
			<!-- Total -->
			<xsd:element name="Total" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total</Description>
						<LineNumber>col (A)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<xsd:element name="ProgramServices" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Program services</Description>
						<LineNumber>col (B)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:group ref="Form990PartIXGroup2" />	

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Group for Part IX, All Lines with Line Description -->
	<xsd:complexType name="Form990PartIXGroup4Type">
		<xsd:sequence>

			<!-- Description -->
			<xsd:element name="Description" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		
			<xsd:group ref="Form990PartIXGroup1"/>		
			<xsd:group ref="Form990PartIXGroup2"/>			

		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Part X, Repeating Group for Balance Sheet Items, Optional -->
	<xsd:complexType name="Form990PartXGroup1Type">
		<xsd:sequence>
		
			<!-- Beginnning of Year -->
			<xsd:element name="BOY" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginnning of year</Description>
						<LineNumber>Part X Column (A)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Ending of Year -->
			<xsd:element name="EOY" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ending of year</Description>
						<LineNumber>Part X Column (B)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Part X, Repeating Group for Balance Sheet Items, Required -->
	<xsd:complexType name="Form990PartXGroup2Type">
		<xsd:sequence>
		
			<!-- Beginnning of Year -->
			<xsd:element name="BOY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginnning of year</Description>
						<LineNumber>Part X Column (A)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Ending of Year -->
			<xsd:element name="EOY" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ending of year</Description>
						<LineNumber>Part X Column (B)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for all Count type elements (Number of something counts/tallys) -->
	<xsd:simpleType name="CountType">
		<xsd:restriction base="IntegerNNType">
			<xsd:totalDigits value="6"/>
		</xsd:restriction>
	</xsd:simpleType>


	<!-- =========================================================================== -->
	<!-- ============= End of Local types, types for repeating groups etc. ================ -->
	<!-- =========================================================================== -->
</xsd:schema>
