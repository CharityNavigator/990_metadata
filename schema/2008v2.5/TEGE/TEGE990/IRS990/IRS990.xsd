					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Total, Column D -->
			<xsd:element name="TotalReportableCompFromOrg" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total, column D</Description>
						<LineNumber>Part VII Section A Line 1b D</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total, Column E -->
			<xsd:element name="TotalReportableCompFrmRltdOrgs" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total, column E</Description>
						<LineNumber>Part VII Section A Line 1b E</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
