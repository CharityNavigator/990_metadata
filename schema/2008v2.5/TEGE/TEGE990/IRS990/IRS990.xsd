			<xsd:element name="GovernmentGrants" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Government grants (contributions)</Description>
						<LineNumber>Part VIII Line 1e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

