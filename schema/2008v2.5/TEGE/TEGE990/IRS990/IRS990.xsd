					<xsd:documentation>
						<Description>Compensation process CEO?</Description>
						<LineNumber>Part VI Section B Line 15a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Process Other? -->
			<xsd:element name="CompensationProcessOther" type="BooleanType" minOccurs="0">
