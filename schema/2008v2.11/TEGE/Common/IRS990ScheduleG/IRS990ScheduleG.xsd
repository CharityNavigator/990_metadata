<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.irs.gov/efile" xmlns="http://www.irs.gov/efile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Tax Exempt and Government Entities Schema - IRS Form 990 Schedule G</Description>
			<TaxYear>2008</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>May 7, 2010</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>

	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>
	
	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule G ========================== -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleG">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule G</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleGType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleG">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule G (Form 990) Supplemental Information Regarding Fundraising or Gaming Activities -->

	<xsd:complexType name="IRS990ScheduleGType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule G</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- Part I Fundraising Activities -->
			<!-- Indicate whether the organization raised funds through any of the following activities. (Check all that apply) -->
			<!-- Mail Solicitations -->
			<xsd:element name="MailSolicitations" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Mail solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Email Solicitations -->
			<xsd:element name="EmailSolicitations" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Email solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Phone Solicitations -->
			<xsd:element name="PhoneSolicitations" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Phone solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- In-Person Solicitations -->
			<xsd:element name="InPersonSolicitations" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>In-person solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Solicitation of Non-Govt Grants -->
			<xsd:element name="SolicitationOfNonGovtGrants" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Solicitation of non-govt grants</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Solicitation of Governmentt Grants -->
			<xsd:element name="SolicitationOfGovtGrants" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Solicitation of government grants</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Special Fundraising Events -->
			<xsd:element name="SpecialFundraisingEvents" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Special fundraising events</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Agreement Reference Fundraising? -->
			<xsd:element name="AgreementRefFundraising" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization have a written or oral agreement with any individual (including officers, directors, trustees or key employees listed in Form 990, Part VII) or entity in connection with professional fundraising activities?</Description>
						<LineNumber>Part I Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fundraiser Activity Information -->
			<xsd:element name="FundraiserActivityInformation" type="Form990SchGPartIGroup1Type" minOccurs="0" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If Line 2a is "Yes," list the ten highest paid individuals or entities (fundraisers) pursuant to agreements under which the fundraiser is to be compensated at least $5,000 by the organization</Description>
						<LineNumber>Part I Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Gross Receipts -->
			<xsd:element name="TotalGrossReceipts" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total gross receipts</Description>
						<LineNumber>Part I Line 2b(iv)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Retained by Contractors -->
			<xsd:element name="TotalRetainedByContractors" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total retained by contractors</Description>
						<LineNumber>Part I Line 2b(v)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Net to Organization -->
			<xsd:element name="TotalNetToOrganization" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net to organization</Description>
						<LineNumber>Part I Line 2b(vi)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Licensed States -->
			<xsd:element name="LicensedStates" type="StateType" minOccurs="0" maxOccurs="60">
				<xsd:annotation>
					<xsd:documentation>
						<Description>List all states in which the organization is registered or licensed to solicit funds or has been notified it is exempt from registration or licensing</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		
			<!-- Part II Events -->
			<!-- Events Information -->
			<xsd:element name="EventsInformation" type="Form990SchGPartIIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete if the organization answered "Yes" to Form 990, Part IV, line 18, or reported more than $15,000 on Form 990-EZ, line 6a</Description>
						<LineNumber>Part II</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part III  -->
			<!-- Gaming Information -->
			<xsd:element name="GamingInformation" type="Form990SchGPartIIIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete this part if the organization reported more than $15,000 on Form 990, Part VIII, line 9a or on Form 990-EZ, line 6a</Description>
						<LineNumber>Part III Lines 1-8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- States Where Gaming Conducted? -->
			<xsd:element name="StatesWhereGamingConducted" type="StateType" minOccurs="0" maxOccurs="60">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter state where organization conducts gaming activities</Description>
						<LineNumber>Part III Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Licensed? -->
			<xsd:element name="Licensed" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is organization licensed in each state?</Description>
						<LineNumber>Part III Line 9a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Explanation If No (License) -->
			<xsd:element name="ExplanationIfNo" type="ShortExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Explanation if no license</Description>
						<LineNumber>Part III Line 9b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- License Suspended, Etc.? -->
			<xsd:element name="LicenseSuspendedEtc" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Were any gaming licenses revoked, suspended, or terminated during the tax year?</Description>
						<LineNumber>Part III Line 10a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Explanation If Yes -->
			<xsd:element name="ExplanationIfYes" type="ShortExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Explanation if license revoked, suspended, or termination</Description>
						<LineNumber>Part III Line 10b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Gaming With Nonmembers? -->
			<xsd:element name="GamingWithNonmembers" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Does organization operate gaming activities with nonmembers?</Description>
						<LineNumber>Part III Line 11</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Member Of Other Entity? -->
			<xsd:element name="MemberOfOtherEntity" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is organization formed to administer charitable gaming?</Description>
						<LineNumber>Part III Line 12</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Percentage Of Gaming In Own Facility -->
			<xsd:element name="PercentageOfGamingOwnFacility" type="RatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Percentage of gaming in own facility</Description>
						<LineNumber>Part III Line 13a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Percentage Of Gaming In Other Facility -->
			<xsd:element name="PercentageOfGamingOthrFacility" type="RatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Percentage of gaming in other facility</Description>
						<LineNumber>Part III Line 13b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Name of Gaming Records Keeper -->
			<xsd:choice minOccurs="0">
				<xsd:element name="NameOfGamingRecKeeperPerson" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Person name of gaming records keeper</Description>
							<LineNumber>Part III Line 14</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="NameOfGamingRecKeeperBus" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Business name of gaming records keeper</Description>
							<LineNumber>Part III Line 14</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Address of Gaming Records Keeper -->
			<xsd:choice minOccurs ="0">

				<xsd:element name="AddressOfGamingRecKeeperUS" type="USAddressType">			
					<xsd:annotation>
						<xsd:documentation>
							<Description>US address of gaming records keeper</Description>
							<LineNumber>Part III Line 14</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="AddressOfGamingRecKeeperFrgn" type="ForeignAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign address of gaming records keeper</Description>
							<LineNumber>Part III Line 14</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Contract With 3rd Party For Gaming Revenue? -->
			<xsd:element name="ContractWith3rdPartyForGamRev" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is there a third party contract for gaming revenue?</Description>
						<LineNumber>Part III Line 15a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount Of Gaming Revenue Received By Organization -->
			<xsd:element name="AmtOfGamingRevenueRecvdByOrg" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount of gaming revenue received by organization</Description>
						<LineNumber>Part III Line 15b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount Of Gaming Revenue Retained By 3rd Party -->
			<xsd:element name="AmtOfGamingRevenueRetndBy3Prty" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount of gaming revenue retained by 3rd party</Description>
						<LineNumber>Part III Line 15b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Name of Third Party -->
			<xsd:choice minOccurs ="0">
				<xsd:element name="NameOfThirdPartyPerson" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Person name of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="NameOfThirdPartyBusiness" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Business name of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Address of Third Party -->
			<xsd:choice minOccurs ="0">
				<xsd:element name="AddressOfThirdPartyUS" type="USAddressType">			
					<xsd:annotation>
						<xsd:documentation>
							<Description>US address of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="AddressOfThirdPartyForeign" type="ForeignAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign address of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
	
			<!-- Gaming Manager Name -->
			<xsd:choice minOccurs ="0">
				<xsd:element name="GamingManagerNamePerson" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Gaming manager person name</Description>
							<LineNumber>Part III Line 16</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="GamingManagerNameBusiness" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Gaming manager business name</Description>
							<LineNumber>Part III Line 16</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Gaming Manager Compensation -->
			<xsd:element name="GamingManagerCompensation" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gaming manager compensation</Description>
						<LineNumber>Part III Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Gaming Manager Services Provided -->
			<xsd:element name="GamingManagerServicesProvided" type="ShortExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gamaing manager services provided</Description>
						<LineNumber>Part III Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Gaming Manager Is A Director/Officer -->
			<xsd:element name="GamingManagerIsDirectorOfficer" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gaming manager is a director or officer</Description>
						<LineNumber>Part III Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Gaming Manager Is An Employee -->
			<xsd:element name="GamingManagerIsEmployee" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gaming manager is an employee</Description>
						<LineNumber>Part III Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Gaming Manager Is An Independent Contractor -->
			<xsd:element name="GamingManagerIsIndContractor" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gaming manager is an independent contractor</Description>
						<LineNumber>Part III Line 16</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charitable Distributions Required? -->
			<xsd:element name="CharitableDistribRequired" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charitable distributions required?</Description>
						<LineNumber>Part III Line 17a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount Distributed -->
			<xsd:element name="AmountDistributed" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount distributed</Description>
						<LineNumber>Part III Line 17b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- =========================================================================== -->
	<!-- ============= Local types, types for repeating groups etc. ================ -->
	<!-- =========================================================================== -->
	<!-- Type for Part I, Line 1b, Columns (i) through (v) -->
	<xsd:complexType name="Form990SchGPartIGroup1Type">
		<xsd:sequence>
	
			<!-- Name of Individual or Organization -->
			<xsd:choice minOccurs ="0">
				<xsd:element name="NameOfIndividual" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Name of individual</Description>
							<LineNumber>Line 2b Column (i)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="NameOfOrganization" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Name of entity (fundraiser)</Description>
							<LineNumber>Line 2b Column (i)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Activity -->
			<xsd:element name="Activity" type="ShortExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Activity</Description>
						<LineNumber>Line 2b Column (ii)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fundraiser Control of Funds? -->
			<xsd:element name="FundraiserControlOfFunds" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fundraiser control of funds?</Description>
						<LineNumber>Line 2b Column (iii)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Gross Receipts -->
			<xsd:element name="GrossReceipts" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gross receipts from activity</Description>
						<LineNumber>Part I Line 2b(iv)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount Retained By Contractor -->
			<xsd:element name="AmountRetainedByContractor" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount paid to (or retained by) fundraiser listed in (i)</Description>
						<LineNumber>Part I Line 2b(v)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Net to Organization -->
			<xsd:element name="NetToOrganization" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount paid to (or retained by) organization</Description>
						<LineNumber>Part I Line 2b(vi)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
	</xsd:complexType>
			

	<!-- Type for Part II, Lines 1 through 9, Columns (a) through (d) -->
	<xsd:complexType name="Form990SchGPartIIGroup1Type">
		<xsd:sequence>
			
			<!-- Name of Event # 1 -->
			<xsd:element name="NameOfEvent1" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Name of event no.1</Description>
						<LineNumber>Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
				
			<!-- Gross Receipts, Event #1 -->
			<xsd:element name="GrossReceiptsEvent1" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gross receipts, event no.1</Description>
						<LineNumber>Line 1 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charitable Contributions, Event #1 -->
			<xsd:element name="CharitableContributionsEvent1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charitable contributions, event no.1</Description>
						<LineNumber>Line 2 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
				
			<!-- Gross Revenue, Event #1 -->
			<xsd:element name="GrossRevenueEvent1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Gross revenue, event no.1</Description>
						<LineNumber>Line 3 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Cash Prizes, Event #1 -->
			<xsd:element name="CashPrizesEvent1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Cash prizes, event no.1</Description>
						<LineNumber>Line 4 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Non-Cash Prizes, Event #1 -->
			<xsd:element name="NonCashPrizesEvent1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Non-cash prizes, event no.1</Description>
						<LineNumber>Line 5 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Rent or facility Costs, Event #1 -->
			<xsd:element name="RentFacilityCostsEvent1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Rent or facility costs, event no.1</Description>
						<LineNumber>Line 6 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Direct Expenses, Event #1 -->
			<xsd:element name="OtherDirectExpensesEvent1" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other direct expenses, event no.1</Description>
						<LineNumber>Line 7 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Name of Event # 2 -->
			<xsd:element name="NameOfEvent2" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Name of event no.2</Description>
						<LineNumber>Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
				
