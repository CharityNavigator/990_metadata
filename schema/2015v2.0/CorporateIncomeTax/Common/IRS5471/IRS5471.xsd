						<!-- Amount of Tax in US Dollars -->
						<xsd:element name="TaxInUSDollarsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of tax in US dollars</Description>
									<LineNumber>Schedule E Line 2, Column (d)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Income and War Profits Taxes -->
			<xsd:element name="TotalIncomeAndWarProfitsTxsAmt" type="USAmountType" minOccurs="0">
