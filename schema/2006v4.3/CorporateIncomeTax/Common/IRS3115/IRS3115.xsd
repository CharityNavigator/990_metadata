			</xsd:element>

			<!-- Other Bidding Expenses -->
			<xsd:element name="OtherBiddingExpenses" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Bidding expenses not included on line 22 above</Description>
						<LineNumber>Schedule D Part III Section C Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- General and Administrative Costs -->
			<xsd:element name="GeneralAndAdministrativeCosts" type="PresentAndProposedMethodCheckboxesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>General and administrative costs not included in Section B above</Description>
