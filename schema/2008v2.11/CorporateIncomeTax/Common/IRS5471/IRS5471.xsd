<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema targetNamespace="http://www.irs.gov/efile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.irs.gov/efile" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xsd:annotation>
		<xsd:documentation>
			<Description>IRS e-file Schema - Form 5471</Description>
			<TaxYear>2008</TaxYear>
			<MaturityLevel>Final Release</MaturityLevel>
			<ReleaseDate>May 7, 2010</ReleaseDate>
		</xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="../../../Common/efileTypes.xsd"/>


	<!-- ================================================================== -->
	<!-- ======================  IRS Form 5471 ============================ -->
	<!-- ================================================================== -->


	<xsd:element name="IRS5471">
		<xsd:annotation>
			<xsd:documentation>IRS Form 5471</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS5471Type">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS5471">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="referenceDocumentId" type="IdListType" />
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS3115 IRS5452 IRS5471ScheduleJ IRS5471ScheduleM IRS5471ScheduleO IRS8594 IRS8833 IRS8883 IRS8886 BinaryAttachment" />

					<xsd:attribute name="dormantIndicator">
						<xsd:simpleType>
							<xsd:restriction base="StringType">
								<xsd:enumeration value="FILED PURSUANT TO REV. PROC. 92-70 FOR DORMANT FOREIGN CORPORATION"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="changeAnnualAccountingPeriod">
						<xsd:simpleType>
							<xsd:restriction base="StringType">
								<xsd:enumeration value="FiledUnderRevProc2006-45"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>

				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="IRS5471Type">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 5471</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Tax Year Beginning -->
			<xsd:element name="TaxYearBeginning" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax year beginning</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Tax Year Ending -->
			<xsd:element name="TaxYearEnding" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Tax year ending</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Change in Taxable Year - No Section 898c(1)(B) -->
			<xsd:element name="ChangeInTaxableYearNo898c" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Change in taxable year - No section 898c(1)(B)</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Election - Change in Year 898c(1)(B) -->
			<xsd:element name="ElectionChangeInYear898c" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Election - Change in year 898c(1)(B)</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Section 898c(1)(B) Election -->
			<xsd:element name="Section898cElection" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 898c(1)(B) election</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Section 338(g) Election -->
			<xsd:element name="Section338gElection" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section 338(g) Election</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Person Filing This Return -->
			<xsd:element name="PersonFilingThisReturn">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Person filing this return</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="Name" type="BusinessNameType" minOccurs="0"/>

						<!-- Address - choice between US Address or Foreign Address -->
						<xsd:choice minOccurs="0">

							<xsd:element name="USAddress" type="USAddressType"/>

							<xsd:element name="ForeignAddress" type="ForeignAddressType"/>

						</xsd:choice>
					</xsd:sequence>
					<xsd:attribute name="referenceDocumentId" type="IdListType"/>
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="NameChangeExplanation" />
				</xsd:complexType>
			</xsd:element>

			<!-- Filers Tax Year Beginning -->
			<xsd:element name="FilersTaxYearBeginning" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Filers tax year beginning</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Filers Tax Year Ending -->
			<xsd:element name="FilersTaxYearEnding" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Filers tax year ending</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Identifying Number: choice between EIN, or "APPLD FOR" or "FOREIGNUS" -->
			<xsd:choice minOccurs="0">

				<!-- EIN -->
				<xsd:element name="EIN" type="EINType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>EIN</Description>
							<LineNumber>A</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Missing EIN Reason -->
				<xsd:element name="MissingEINReason">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Missing EIN reason</Description>
							<LineNumber>A</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="StringType">
							<xsd:enumeration value="APPLD FOR"/>
							<xsd:enumeration value="FOREIGNUS"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>

			</xsd:choice>

			<!-- Category of Filer 2 -->
			<xsd:element name="CategoryOfFiler2" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Category of filer 2</Description>
						<LineNumber>B(2)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Category of Filer 3 -->
			<xsd:element name="CategoryOfFiler3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Category of filer 3</Description>
						<LineNumber>B(3)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="Category3FilerStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Category of Filer 4 -->
			<xsd:element name="CategoryOfFiler4" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Category of filer 4</Description>
						<LineNumber>B(4)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Category of Filer 5 -->
			<xsd:element name="CategoryOfFiler5" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Category of filer 5</Description>
						<LineNumber>B(5)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Percentage of Voting Stock Owned -->
			<xsd:element name="PercentageOfVotingStockOwned" type="RatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Percentage of voting stock owned</Description>
						<LineNumber>C</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Person(s) on Whose Behalf this Information is Filed -->
			<xsd:element name="PersonInformationIsFiledFor" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
							<Description>Person(s) on whose behalf this information is filed</Description>
						<LineNumber>D</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="NameAndAddressType">
							<xsd:sequence>

								<!-- Identifying Number of Person This Information Return is Filed For: choice between EIN, or "APPLD FOR" or "FOREIGNUS" -->
								<xsd:choice minOccurs="0">

									<!-- EIN -->
									<xsd:element name="EIN" type="EINType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>EIN</Description>
												<LineNumber>D(3)</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

									<!-- Missing EIN Reason -->
									<xsd:element name="MissingEINReason">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Missing EIN reason</Description>
												<LineNumber>D(3)</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="StringType">
												<xsd:enumeration value="APPLD FOR"/>
												<xsd:enumeration value="FOREIGNUS"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>

								</xsd:choice>

								<!-- Shareholder Indicator of Person This Information Return is Filed For  -->
								<xsd:element name="ShareholderIndicator" type="CheckboxType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>
											<Description>Shareholder indicator of person this information return is filed for</Description>
											<LineNumber>D(4)</LineNumber>
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

								<!-- Officer Indicator of Person This Information Return is Filed For  -->
								<xsd:element name="OfficerIndicator" type="CheckboxType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>
											<Description>Officer indicator of person this information return is filed for</Description>
											<LineNumber>D(4)</LineNumber>
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

								<!-- Director Indicator of Person This Information Return is Filed For  -->
								<xsd:element name="DirectorIndicator" type="CheckboxType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>
											<Description>Director indicator of person this information return is filed for</Description>
											<LineNumber>D(4)</LineNumber>
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

							</xsd:sequence>

						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Foreign Corporation -->
			<xsd:element name="ForeignCorporation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation</Description>
						<LineNumber>1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="BusinessName" type="BusinessNameType" minOccurs="0"/>

						<xsd:choice minOccurs="0">
							<xsd:element name="AddressInUS" type="USAddressType"/>
							<xsd:element name="AddressInForeign" type="ForeignAddressType"/>
						</xsd:choice>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Employer Identifying Number: choice between EIN, or "APPLD FOR" or "FOREIGNUS" -->
			<xsd:choice minOccurs="0">

				<!-- Employer EIN -->
				<xsd:element name="EmployerEIN" type="EINType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Employer EIN</Description>
							<LineNumber>1b</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Missing Employer EIN Reason -->
				<xsd:element name="MissingEmployerEINReason">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Missing Employer EIN reason</Description>
							<LineNumber>1b</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="StringType">
							<xsd:enumeration value="APPLD FOR"/>
							<xsd:enumeration value="FOREIGNUS"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>

			</xsd:choice>

			<!-- Country Under Whose Laws Incorporated -->
			<xsd:element name="CountryUnderWhoseLawsInc" type="CountryType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Country under whose laws incorporated</Description>
						<LineNumber>1c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Date of Incorporation -->
			<xsd:element name="DateOfIncorporation" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Date of incorporation</Description>
						<LineNumber>1d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Principal Place of Business -->
			<xsd:element name="PrincipalPlaceOfBusiness" type="AllCountriesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Principal place of business</Description>
						<LineNumber>1e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Principal business activity code or Inactive -->
			<xsd:choice minOccurs="0">

				<!-- Principal Business Activity Code -->
				<xsd:element name="PrincipalBusinessActivityCode" type="BusinessActivityCodeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Principal business activity code</Description>
							<LineNumber>1f</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Inactive principal business activity code -->
				<xsd:element name="InactivePrincipalBusActyCode">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Inactive Principal business activity code</Description>
							<LineNumber>1f</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="StringType">
							<xsd:enumeration value="Inactive"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>

			</xsd:choice>

			<!-- Principal Business Activity -->
			<xsd:element name="PrincipalBusinessActivity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Principal business activity</Description>
						<LineNumber>1g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
						<xsd:pattern value="[A-Za-z0-9\- &amp;]*"/>
						<xsd:maxLength value="35"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>

			<!-- Foreign Corporation's Functional Currency -->
			<xsd:element name="FunctionalCurrency" type="ShortDescriptionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation's functional currency</Description>
						<LineNumber>1h</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Branch Office in the United States -->
			<xsd:element name="BranchOfficeInTheUS" type="NameAndAddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Branch office in the united states</Description>
						<LineNumber>2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Identifying Number of Branch Office in the United States: choice between EIN, or "APPLD FOR" or "FOREIGNUS" -->
			<xsd:choice minOccurs="0">

				<!-- EIN of Branch Office in the United States -->
				<xsd:element name="EINOfBranchOfficeInTheUS" type="EINType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>EIN of branch office in the United States</Description>
							<LineNumber>2a</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Missing EIN Reason of Branch Office in the United States -->
				<xsd:element name="MissingEINBranchOfficeInTheUS">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Missing EIN reason of branch office in the United States</Description>
							<LineNumber>2a</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="StringType">
							<xsd:enumeration value="APPLD FOR"/>
							<xsd:enumeration value="FOREIGNUS"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>

			</xsd:choice>

			<!-- Taxable Income or Loss -->
			<xsd:element name="TaxableIncomeOrLoss" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable income or loss</Description>
						<LineNumber>2b(i)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- US Income Tax Paid After All Credits -->
			<xsd:element name="USIncomeTaxPaidAfterAllCredits" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>US income tax paid after all credits </Description>
						<LineNumber>2b(ii)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Corporation's Statutory or Resident Agent -->
			<xsd:element name="FrgnCorpStatutoryOrResidentAgt" type="NameAndAddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation's statutory or resident agent</Description>
						<LineNumber>2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Person with Custody of Books and Records of Foreign Corporation -->
			<xsd:element name="PersonWithRecordsCustody"  minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Person with custody of books and records of foreign corporation</Description>
						<LineNumber>2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>

				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="NameAndAddressType">
							<xsd:sequence>
								<!-- Address of Location Of Books And Records -->
								<xsd:choice minOccurs="0">
									<xsd:element name="LocationAddressInUS" type="USAddressType"/>
									<xsd:element name="LocationAddressInForeign" type="ForeignAddressType"/>
								</xsd:choice>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>

			<!-- IRS5471 Schedule A - Stock of the Foreign Corporation -->
			<xsd:element name="IRS5471ScheduleA" type="IRS5471ScheduleAType" minOccurs="0" />

			<!-- IRS5471 Schedule B - U.S. Shareholders of Foreign Corporation -->
			<xsd:element name="IRS5471ScheduleB" type="IRS5471ScheduleBType" minOccurs="0" />

			<!-- IRS5471 Schedule C - Income Statement -->
			<xsd:element name="IRS5471ScheduleC" type="IRS5471ScheduleCType" minOccurs="0" />

			<!-- IRS5471 Schedule E - Income, War Profits, and Excess Profits Taxes Paid or Accrued -->
			<xsd:element name="IRS5471ScheduleE" type="IRS5471ScheduleEType" minOccurs="0" />

			<!-- IRS5471 Schedule F - Balance Sheet -->
			<xsd:element name="IRS5471ScheduleF" type="IRS5471ScheduleFType" minOccurs="0" />

			<!-- IRS5471 Schedule G - Other Information -->
			<xsd:element name="IRS5471ScheduleG" type="IRS5471ScheduleGType" minOccurs="0" />

			<!-- IRS5471 Schedule H - Current Earnings and Profits -->
			<xsd:element name="IRS5471ScheduleH" type="IRS5471ScheduleHType" minOccurs="0" />

			<!-- IRS5471 Schedule I - Summary of Shareholder's Income From Foreign Corporation -->
			<xsd:element name="IRS5471ScheduleI" type="IRS5471ScheduleIType" minOccurs="0" maxOccurs="unbounded" />

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule A ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleAType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 5471 Schedule A</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- Schedule A - Stock of the Foreign Corporation  -->

			<xsd:element name="StockOfTheForeignCorporation" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Stock of the Foreign Corporation</Description>
						<LineNumber>Schedule A</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>

					<xsd:sequence>

						<!-- Description of Each Class of Stock: choice between Description, or "COMMON" or "PREFERRED" or "TREASURY" -->
						<xsd:choice minOccurs="0">
							<!-- Description of Each Class of Stock -->
							<xsd:element name="DescOfEachClassOfStock" type="StockDescriptionType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Description of Each Class of Stock</Description>
										<LineNumber>Schedule A Column (a)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<!-- Selection of Each Class of Stock "COMMON" or "PREFERRED" or "TREASURY" -->
							<xsd:element name="SelectionOfEachClassOfStock">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Selection of each class of stock Common, Preferred, Treasury</Description>
										<LineNumber>Schedule A Column (a)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="StringType">
										<xsd:enumeration value="COMMON"/>
										<xsd:enumeration value="PREFERRED"/>
										<xsd:enumeration value="TREASURY"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>

						</xsd:choice>

						<!-- Number of Shares Issued Beginning Annual Accounting Period -->
						<xsd:element name="NumOfSharesBegngAnnualAcctPrd" type="NumberOfSharesType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Number of shares issued beginning annual accounting period</Description>
									<LineNumber>Schedule A Column b(i)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Number of Shares Issued End Annual Accounting Period -->
						<xsd:element name="NumberOfSharesEndAnnualAcctPrd" type="NumberOfSharesType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Number of shares issued end annual accounting period</Description>
									<LineNumber>Schedule A Column b(ii)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>

			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================ -->
	<!-- ======================  IRS Form 5471 Schdule B ============================ -->
	<!-- ============================================================================ -->

	<xsd:complexType name="IRS5471ScheduleBType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 5471 Schedule B</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- US Shareholders of Foreign Coproration -->
			<xsd:element name="USShareholdersOfForeignCorp" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>US shareholders of foreign corporation</Description>
						<LineNumber>Schedule B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="NameAndAddressType">
							<xsd:sequence>

								<!-- Identifying Number of Shareholder: choice between EIN, SSN, or "APPLD FOR" or "FOREIGNUS" -->
								<xsd:choice minOccurs="0">

									<!-- EIN of Shareholder -->
									<xsd:element name="EINOfShareholder" type="EINType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>EIN of shareholder</Description>
												<LineNumber>Schedule B Column (a)</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

									<!-- SSN of Shareholder -->
									<xsd:element name="SSNOfShareholder" type="SSNType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>SSN of shareholder</Description>
												<LineNumber>Schedule B Column (a)</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

									<!-- Missing EIN Reason of Shareholder -->
									<xsd:element name="MissingEINReasonOfShareholder">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Missing EIN reason of shareholder</Description>
												<LineNumber>Schedule B Column (a)</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="StringType">
												<xsd:enumeration value="APPLD FOR"/>
												<xsd:enumeration value="FOREIGNUS"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>

								</xsd:choice>

								<!-- Foreign Corporation Stocks -->
								<xsd:element name="ForeignCorporationStocks" type="ForeignCorporationStocksType" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>
											<Description>Foreign Corporation Stocks</Description>
											<LineNumber>Schedule B Columns (b) through (d)</LineNumber>
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

								<!-- Pro Rata Share of Subpart F Income -->
								<xsd:element name="ProRataShareOfSubpartFIncome" type="RatioType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation>
											<Description>Pro rata share of subpart F income</Description>
											<LineNumber>Schedule B Column (e)</LineNumber>
										</xsd:documentation>
									</xsd:annotation>
								</xsd:element>

							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>


		</xsd:sequence>
	</xsd:complexType>


