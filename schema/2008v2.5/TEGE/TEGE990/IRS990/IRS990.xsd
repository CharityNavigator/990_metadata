					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Part VII, Contractor Compensation -->
	<xsd:complexType name="Form990PartVIIGroup1Type">
		<xsd:sequence>

			<!-- Name of Contractor -->
			<xsd:element name="NameOfContractor" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Name of contractor</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<!-- Name Choice -->
						<xsd:choice minOccurs="0">
							<xsd:element name="NamePerson" type="PersonNameType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Name - Person</Description>
										<LineNumber>Part VII Section B Line 1(A)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="NameBusiness" type="BusinessNameType">
							<xsd:annotation>
									<xsd:documentation>
										<Description>Name - Business</Description>
										<LineNumber>Part VII Section B Line 1(A)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:sequence>						
				</xsd:complexType>							
			</xsd:element>

			<!-- Address of Contractor -->
			<xsd:element name="AddressOfContractor" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Address of contractor</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<!-- Address Choice -->
						<xsd:choice minOccurs="0">
							<xsd:element name="AddressUS" type="USAddressType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Address - US</Description>
										<LineNumber>Part VII Section B Line 1(A)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="AddressForeign" type="ForeignAddressType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Address - Foreign</Description>
										<LineNumber>Part VII Section B Line 1(A)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>
					</xsd:sequence>						
				</xsd:complexType>							
			</xsd:element>

			<!-- Description of Services -->
			<xsd:element name="DescriptionOfServices" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description of services</Description>
						<LineNumber>Part VII Section B Line 1(B)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation -->
			<xsd:element name="Compensation" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation</Description>
						<LineNumber>Part VII Section B Line 1(C)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<!-- Group for Common Repeating elements in Part VIII Cols (A) - (D) -->
	<xsd:group name="Form990PartVIIIMainGroup">
		<xsd:sequence>
			<!-- Total Revenue -->
			<xsd:element name="TotalRevenueColumn" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total revenue</Description>
						<LineNumber>Part VIII Column (A)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Related or Exempt Function Revenue -->
			<xsd:element name="RelatedOrExemptFunctionIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Related or exempt function revenue</Description>
						<LineNumber>Part VIII Column (B)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Unrelated Business Revenue -->
			<xsd:element name="UnrelatedBusinessRevenue" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Unrelated business revenue</Description>
						<LineNumber>Part VIII Column (C)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Excluded by Section 512, 513, or 514: Amount -->
			<xsd:element name="ExclusionAmount" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Excluded by section 512, 513, or 514: amount</Description>
						<LineNumber>Part VIII Column (D)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		
		</xsd:sequence>
	</xsd:group>

	<!-- Group for Common Repeating element in Part VIII Business Code -->
	<xsd:group name="Form990PartVIIIGroup1">
		<xsd:sequence>
			<!-- Unrelated Business Income: Business Code -->
			<xsd:element name="BusinessCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Business code</Description>
						<LineNumber>Part VIII</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
						<xsd:pattern value="[0-9]{6}"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<!-- Other Repeating Elements -->
			<xsd:group ref="Form990PartVIIIMainGroup"/>			
		</xsd:sequence>
	</xsd:group>

	<!-- Type for Part VIII, Repeating Group with Business Code and Main Elements -->
	<xsd:complexType name="Form990PartVIIIGroup1Type">
		<xsd:sequence>
			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990PartVIIIGroup1"/>
		</xsd:sequence>
	</xsd:complexType>

			
	<!-- Type for Part VIII, Repeating Group with Description Element, Business Code, and Main -->
	<xsd:complexType name="Form990PartVIIIGroup2Type">
		<xsd:sequence>
			<!-- Description -->
			<xsd:element name="Description" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other Repeating Elements -->
			<xsd:group ref="Form990PartVIIIGroup1"/>
		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Part VIII, Repeating Main Group without Anything Else -->
