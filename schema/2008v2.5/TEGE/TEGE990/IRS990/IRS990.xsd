			<xsd:element name="NoListedPersonsCompensated" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>No listed persons compensated</Description>
						<LineNumber>Part VII Section A</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="Form990PartVIISectionA" minOccurs="1" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Section A, at least name required</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
					
						<!-- Name -->
						<!-- Name Choice -->
						<xsd:choice>
							<xsd:element name="NamePerson" type="PersonNameType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Name - Person</Description>
										<LineNumber>Part VII Section A Line 1a A</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="NameBusiness" type="BusinessNameType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Name - Business</Description>
										<LineNumber>Part VII Section A Line 1a A</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>

