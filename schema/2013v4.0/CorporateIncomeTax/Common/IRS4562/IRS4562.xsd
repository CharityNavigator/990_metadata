			<!-- Amortization Period or Percentage -->
			<xsd:element name="AmortizationPeriodOrPctTxt" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amortization period or percentage</Description>
						<LineNumber>42e</LineNumber>
