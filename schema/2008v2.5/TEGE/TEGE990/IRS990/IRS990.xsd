			<xsd:element name="TotalProgramServiceRevenue" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Program service revenue total</Description>
						<LineNumber>Part VIII Line 2g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

