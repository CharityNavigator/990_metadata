	<!-- Type for Present Method and Proposed Method checkboxes in Schedule D, Part III, Sections A, B & C-->
	<xsd:complexType name="PresentAndProposedMethodCheckboxesType">
		<xsd:sequence>

			<!-- Present Method -->
			<xsd:element name="PresentMethod" type="CheckOrNAType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Present method</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Proposed Method -->
			<xsd:element name="ProposedMethod" type="CheckOrNAType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Proposed method</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- =============================================================== -->
	<!-- ===================  IRS Form 3115 Schedule E ================= -->
	<!-- =============================================================== -->

	<xsd:complexType name="IRS3115ScheduleEType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 3115 Schedule E</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>

			<!-- Change in Depreciation or Amortization (for each item or class of property for which a change is requested) -->
			<xsd:element name="ChangeInDeprecOrAmortization" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>

						<!-- Property Depreciation Figured Under Section 1.167(a)-11? -->
						<xsd:element name="PropertyDeprecUnderSect1167a11" type="BooleanType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Is depreciation for the property figured under Regulations section 1.167(a)-11 (CLADR)?</Description>
									<LineNumber>Schedule E Line 1</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Any Depreciation or Amortization Required Under Any Code Section? -->
						<xsd:element name="AnyDeprecOrAmortzRqrUnderCode" type="BooleanType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Is any of the depreciation or amortization required to be capitalized under any Code section (e.g., section 263A)?</Description>
									<LineNumber>Schedule E Line 2</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Applicable Section -->
						<xsd:element name="ApplicableSection" type="AlphaNumericAndParenthesesType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>If "Yes", enter the applicable section</Description>
									<LineNumber>Schedule E Line 2</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Depreciation or Amortization Election Been Made? -->
						<xsd:element name="DeprecOrAmortzElectionBeenMade" type="BooleanType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Has a depreciation or amortization election been made for the property (e.g., the election under section 168(f)(1))?</Description>
									<LineNumber>Schedule E Line 3</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Election Made -->
						<xsd:element name="ElectionMade" type="AlphaNumericAndParenthesesType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>If "Yes", state the election made</Description>
									<LineNumber>Schedule E Line 3</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Lived in Property Before Renting It? -->
						<xsd:element name="LivedInPropertyBeforeRentingIt" type="BooleanType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>If the property is residential rental property, did the applicant live in the property before renting it?</Description>
									<LineNumber>Schedule E Line 4b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Public Utility Property? -->
						<xsd:element name="PublicUtilityProperty" type="BooleanType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Is the property public utility property?</Description>
									<LineNumber>Schedule E Line 4c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>

					<xsd:attribute name="referenceDocumentId" type="IdListType" />
					<xsd:attribute name="referenceDocumentName" type="StringType" fixed="DescriptionOfPropertyBeingChangedStatement ExplanationHowPropertyTreatedUnderPresentMethodStatement FactsSuprtProposedChgToDepreciateOrAmortizePropStmt CodeSectUnderWhichPropIsDeprecOrAmortzStatement" />

				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- Type for Contact Information lines throughout this form -->
	<xsd:complexType name="ContactInfoType">
		<xsd:sequence>

			<!-- Name -->
			<xsd:element name="Name" type="PersonNameType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Name</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Phone Number -->
			<xsd:element name="PhoneNumber" type="PhoneNumberType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Phone Number</Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Type for Contact Information and tax year(s)-->
	<xsd:complexType name="ContactAndTaxYearInfoType">
		<xsd:annotation>
			<xsd:documentation>Type for Contact Information and tax year(s)</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ContactInfoType">
				<xsd:sequence>
				
					<!-- Tax year(s) -->
					<xsd:element name="TaxYear" type="YearType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>
								<Description>Tax year(s)</Description>
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					
				</xsd:sequence>
			</xsd:extension>	
		</xsd:complexContent>	
	</xsd:complexType>

	<!-- Type for Alpha-Numeric (plus Space) -->
	<xsd:simpleType name="AlphaNumericAndSpaceType">
		<xsd:restriction base="StringType">
			<xsd:pattern value="[A-Za-z0-9 ]*" />
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Check ('X') or 'NA' Type -->
	<xsd:simpleType name="CheckOrNAType">
		<xsd:restriction base="StringType">
			<xsd:enumeration value="X" />
			<xsd:enumeration value="NA" />
		</xsd:restriction>
	</xsd:simpleType>

</xsd:schema>
