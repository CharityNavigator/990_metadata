						<LineNumber>Schedule F Line 19(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="PaidInOrCapSurplusRecnclStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Beginning Accounting Period Paid-in or Capital Surplus -->
			<xsd:element name="EndAccountingPrdPaidInSurplus" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period paid-in or capital surplus</Description>
						<LineNumber>Schedule F Line 19(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="PaidInOrCapSurplusRecnclStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Beginning Accounting Period Retained Earnings -->
			<xsd:element name="BegngAcctPrdRetainedEarnings" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period retained earnings</Description>
						<LineNumber>Schedule F Line 20(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Retained Earnings -->
			<xsd:element name="EndAcctPeriodRetainedEarnings" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period retained earnings</Description>
						<LineNumber>Schedule F Line 20(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Beginning Accounting Period Retained Earnings Less Cost of Treasury Stock -->
			<xsd:element name="BegngAcctPrdRtnEarnLessTrStock" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period retained earnings less cost of treasury stock</Description>
						<LineNumber>Schedule F Line 21(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Retained Earnings Less Cost of Treasury Stock -->
			<xsd:element name="EndAcctPrdRtnEarnLessTrStock" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period retained earnings less cost of treasury stock</Description>
						<LineNumber>Schedule F Line 21(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Beginning Accounting Period Total Liabilities and Stockholders' Equity -->
			<xsd:element name="BegngAcctPrdTotLiabAndEquity" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning accounting period total liabilities and stockholders' equity</Description>
						<LineNumber>Schedule F Line 22(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- End Accounting Period Total Liabilities and Stockholders' Equity -->
			<xsd:element name="EndAcctPrdTotLiabAndEquity" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>End accounting period total liabilities and stockholders' equity</Description>
						<LineNumber>Schedule F Line 22(b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule G ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleGType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1120 Schedule G</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>
			<!-- Foreign Corporation Owns 10% or More in a Foreign Partnership -->
			<xsd:element name="Owns10PctOrMoreFrgnPartnership">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation own 10% or more in a foreign partnership</Description>
						<LineNumber>Schedule G Line1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="OwnsForeignPartnershipStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Foreign Corporation Owns Interest in Any Trust -->
			<xsd:element name="OwnsInterestInAnyTrust" type="BooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation owns interest in any trust</Description>
						<LineNumber>Schedule G Line2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Foreign Corporation Owns Any Foreign Entities -->
			<xsd:element name="OwnsAnyForeignEntities">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Foreign corporation owns any foreign entities</Description>
						<LineNumber>Schedule G Line3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="IRS8858 OwnsForeignEntitiesStatement" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- The Foreign Corporation Was a Participant in a Cost Sharing Arrangement -->
			<xsd:element name="ForeignCorpWasParticipant" type="BooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The foreign corporation was a participant in a cost sharing arrangement</Description>
						<LineNumber>Schedule G Line4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- The Foreign Corporation Became a Participant in a Cost Sharing Arrangement -->
			<xsd:element name="ForeignCorpBecameParticipant" type="BooleanType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>The foreign corporation became a participant in a cost sharing arrangement</Description>
						<LineNumber>Schedule G Line5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule H ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleHType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1120 Schedule H</xsd:documentation>
		</xsd:annotation>

		<xsd:sequence>
			<!-- Current Year Income Per Foreign Books of Account -->
			<xsd:element name="CYIncomePerForeignBooksOfAcct" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year income per foreign books of account </Description>
						<LineNumber>Schedule H Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Capital Gains Or Losses Net Additions -->
			<xsd:element name="CapitalGainsOrLossNetAddition" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital gains or losses net additions</Description>
						<LineNumber>Schedule H Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Capital Gains Or Losses Net Subtractions -->
			<xsd:element name="CapitalGainsLossNetSubtraction" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Capital gains or losses net subtractions</Description>
						<LineNumber>Schedule H Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depreciation and Amortization Net Additions -->
			<xsd:element name="DeprecAndAmortizNetAddition" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depreciation and amortization net additions</Description>
						<LineNumber>Schedule H Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depreciation and Amortization Net Subtractions -->
			<xsd:element name="DeprecAndAmortizNetSubtraction" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depreciation and amortization net subtractions</Description>
						<LineNumber>Schedule H Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depletion Net Additions -->
			<xsd:element name="DepletionNetAddition" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depletion net additions</Description>
						<LineNumber>Schedule H Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Depletion Net Subtraction -->
			<xsd:element name="DepletionNetSubtraction" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Depletion net subtraction</Description>
						<LineNumber>Schedule H Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investment or Incentive Allowance Net Additions -->
			<xsd:element name="InvstOrIncntvAllowanceNetAddn" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment or incentive allowance net additions</Description>
						<LineNumber>Schedule H Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Investment or Incentive Allowance Net Subtracions -->
			<xsd:element name="InvstOrIncntvAllwncNetSubtrctn" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment or incentive allowance net subtraction</Description>
						<LineNumber>Schedule H Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charges to Statutory Reserves Net Additions -->
			<xsd:element name="ChrgsToStryReservesNetAddition" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charges to statutory reserves net additions</Description>
						<LineNumber>Schedule H Line 2e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Charges to Statutory Reserves Net Subtractions -->
			<xsd:element name="ChrgsToStryRsrvNetSubtraction" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Charges to statutory reserves net subtractions</Description>
						<LineNumber>Schedule H Line 2e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Inventory Adjustments Net Additions -->
			<xsd:element name="InventoryAdjustmentNetAddition" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Inventory adjustments net additions</Description>
						<LineNumber>Schedule H Line 2f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Inventory Adjustments Net Subtractions -->
			<xsd:element name="InventoryAdjNetSubtraction" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Inventory adjustments net subtractions</Description>
						<LineNumber>Schedule H Line 2f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxes Net Additions -->
			<xsd:element name="TaxesNetAddition" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxes net additions</Description>
						<LineNumber>Schedule H Line 2g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxes Net Subtractions -->
			<xsd:element name="TaxesNetSubtraction" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxes net subtractions</Description>
						<LineNumber>Schedule H Line 2g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Adjustments Net Additions -->
			<xsd:element name="OtherAdjustmentsNetAddition" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other adjustments net additions</Description>
						<LineNumber>Schedule H Line 2h</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="ForeignAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="EarningsAndProfitsOtherAdjStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Other Adjustments Net Subtractions -->
			<xsd:element name="OtherAdjNetSubtraction" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other adjustments net subtractions</Description>
						<LineNumber>Schedule H Line 2h</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="ForeignAmountType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="EarningsAndProfitsOtherAdjStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Net Additions -->
			<xsd:element name="TotalNetAdditions" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net additions</Description>
						<LineNumber>Schedule H Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Net Subtractions -->
			<xsd:element name="TotalNetSubtractions" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net subtractions</Description>
						<LineNumber>Schedule H Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Earnings and Profits -->
			<xsd:element name="CurrentEarningsAndProfits" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>current earnings and profits</Description>
						<LineNumber>Schedule H Line 5a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- DASTM Gain or Loss -->
			<xsd:element name="DASTMGainOrLoss" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>DASTM gain or loss</Description>
						<LineNumber>Schedule H Line 5b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Earnings and Profits plus DASTM Gain -->
			<xsd:element name="EarnAndProfitsPlusDASTMGain" type="ForeignAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>current earnings and profits plus DASTM gain</Description>
						<LineNumber>Schedule H Line 5c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Earnings and Profits in US Dollars -->
			<xsd:element name="CurrentEarnAndPrftInUSDollars" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current earnings and profits in US dollars</Description>
						<LineNumber>Schedule H Line 5d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="USAmountType">
							<xsd:attribute name="exchangeRateUsed" type="LargeRatioType"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>


	<!-- ============================================================================= -->
	<!-- ======================  IRS Form 5471 Schedule I ============================ -->
	<!-- ============================================================================= -->

	<xsd:complexType name="IRS5471ScheduleIType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 1120 Schedule I</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Subpart F Income -->
			<xsd:element name="SubpartFIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subpart F income</Description>
						<LineNumber>Schedule I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Earnings Invested in US Property -->
			<xsd:element name="EarningsInvestedInUSProperty" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Earnings invested in US property</Description>
						<LineNumber>Schedule I Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Previously Excluded Subpart F Income-->
			<xsd:element name="PrevExcludedSubpartFIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Previously excluded subpart F income</Description>
						<LineNumber>Schedule I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Previously Excluded Export Trade Income Withrawn from Investment in Export Trade Asset -->
			<xsd:element name="WithdrawnExportTradeIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Previously excluded export trade income withrawn from investment in export trade asset</Description>
						<LineNumber>Schedule I Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Factoring Income -->
			<xsd:element name="FactoringIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Factoring income</Description>
						<LineNumber>Schedule I Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total of lines 1 throug 5 -->
			<xsd:element name="WthdrwPropAndFactoringIncome" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of lines 1 through 5</Description>
						<LineNumber>Schedule I Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Dividends Received -->
			<xsd:element name="DividendsReceived" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Dividends received</Description>
						<LineNumber>Schedule I Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Exchange Gain on a Distribution of Previously Taxed Income -->
			<xsd:element name="ExchangeGainOnDistribution" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Exchange gain on a distribution of previously taxed income</Description>
						<LineNumber>Schedule I Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Was Any Income of the Foreign Corporation Blocked? -->
			<xsd:element name="AnyIncomeBlocked" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Was any income of the foreign corporation blocked?</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="ExplanationOfBlockedIncomeStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Did Any Income Become Unblocked This Tax Year? -->
			<xsd:element name="IncomeBecomeUnblockedThisTY" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did any income become unblocked this tax year?</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="BooleanType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="ExplanationOfBlockedIncomeStmt" />
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- ========================================================================== -->
	<!-- ======================  Recurring Local Types ============================ -->
	<!-- ========================================================================== -->

	<!-- Recurring Stock Description Type -->
	<xsd:simpleType name="StockDescriptionType">
		<xsd:annotation>
			<xsd:documentation>Recurring Stock Description Type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="StringType">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Foreign Corporation Stocks Repeating Group Type -->
	<xsd:complexType name="ForeignCorporationStocksType">
		<xsd:sequence>

			<!-- Description of Stock Held by Shareholder -->
			<xsd:element name="DescOfStockHeldByShareholder" type="StockDescriptionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description of stock held by shareholder</Description>
						<LineNumber>Schedule B Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Shares Issued Beginning Annual Accounting Period -->
			<xsd:element name="NumOfSharesBegngAnnualAcctPrd" type="NumberOfSharesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of shares issued beginning annual accounting period</Description>
						<LineNumber>Schedule B Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of Shares Issued End Annual Accounting Period -->
			<xsd:element name="NumberOfSharesEndAnnualAcctPrd" type="NumberOfSharesType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of shares issued end annual accounting period</Description>
						<LineNumber>Schedule B Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

</xsd:schema>
